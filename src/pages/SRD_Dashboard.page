    <script type="text/javascript">
    function showtstPopup(SRId){        
        var pop=SRId;
              Visualforce.remoting.Manager.invokeAction(
     '{!$RemoteAction.SRDController.getSRpopup}', pop,
        function(result, event)
        {
             if (event.status) 
            {               
                var createddate=new Date(result[0].CreatedDate); 
                var duedate=new Date(result[0].Customer_Due_Date__c);
                var HMRdate=new Date(result[0].Last_HMR_Sync__c );
                $("#text").empty();
                $("#text").append(result[0].Name);
                $("#text1").empty();
                $("#text1").append(result[0].SBU__c);
                $("#text2").empty();
                $("#text2").append(result[0].Subject__c);
                $("#col-md-62").empty();
                $("#col-md-62").append(result[0].Escalation_Tier__c);
                $("#col-md-64").empty();
                $("#col-md-64").append(result[0].Complexity_Level__c);
                $("#col-md-66").empty();
                $("#col-md-66").append(result[0].CSB_Number__c);
                $("#col-md-68").empty();
                $("#col-md-68").append(result[0].Key_Code__c);
                $("#col-md-610").empty();
                $("#col-md-610").append(result[0].Owner_Manager__c);
                $("#col-md-612").empty();
                $("#col-md-612").append(result[0].SR_Site__c);
                $("#col-md-614").empty();
                $("#col-md-614").append(result[0].CBT__c);
                $("#col-md-616").empty();
                $("#col-md-616").append(result[0].Account_Name__r.Name);
                $("#col-md-618").empty();
                $("#col-md-618").append(result[0].SRD_Platform_Name__c);
                if(createddate=="Invalid Date")
                {
                $("#col-md-621").empty();
                }
                else
                {
                $("#col-md-621").empty();
                $("#col-md-621").append(createddate);
                }
                if(duedate=="Invalid Date")
                {
                $("#col-md-623").empty();
                }
                else
                {
                $("#col-md-623").empty();
                $("#col-md-623").append(duedate);
                }
                if(HMRdate=="Invalid Date")
                {
                $("#col-md-625").empty();
                }
                else
                {
                $("#col-md-625").empty();
                $("#col-md-625").append(HMRdate);
                }
                $("#col-md-627").empty();
                $("#col-md-627").append(result[0].Product_Family__c);
                $("#col-md-629").empty();
                $("#col-md-629").append(result[0].Product_Code__c);
                $("#col-md-631").empty();
                $("#col-md-631").append(result[0].Product_Description__c);
                $("#col-md-633").empty();
                $("#col-md-633").append(result[0].ownerid);
                $("#col-md-635").empty();
                $("#col-md-635").append(result[0].PSPM__c);
                $("#col-md-637").empty();
                $("#col-md-637").append(result[0].Performance__c);
                $("#col-md-639").empty();
                $("#col-md-639").append(result[0].Near_Term_Alternate_Fix__c);
                $("#col-md-641").empty();
                $("#col-md-641").append(result[0].Status_Resolution__c  );
                $("#col-md-643").empty();
                $("#col-md-643").append(result[0].Problem_Description__c);                
                $("#col-md-645").empty();
                $("#col-md-645").append(result[0].Customer_Impact__c);
                $("#col-md-646").click( function() { window.top.location = "https://na19.salesforce.com/"+result[0].Id; return false; } );    
                $("#Overlay_-_SR-1113").show();
            }
        
        }, {escape:true});         
              
        }
