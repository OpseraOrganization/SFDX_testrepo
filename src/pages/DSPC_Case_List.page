<apex:page controller="DSPC_Case_List_controller">
    <apex:image id="theImage" value="{!$Resource.DSPC_Case_Home}"/>
    <apex:form >
        <div style="position:relative;">
            <apex:outputpanel >
                <apex:actionstatus id="status">
                    <apex:facet name="start">
                        <div class="waitingSearchDiv" id="el_loading" style="background-color: #FFFFFF; height: 100%; width:100%; opacity: 0.95">
                            <div class="waitingHolder" style="top: 74.2px; width: 100%; background-color: #FFFFFF;">
                                <img class="waitingImage" src="{!$Resource.DSPC_Loading}" title="Please Wait..."/>
                            </div>
                        </div>
                    </apex:facet>
                </apex:actionstatus>
                <apex:pageBlock id="myblock">
                    <apex:pageblocktable id="tableOne" value="{!AllSearchcase}" var="c">
                        <apex:column value="{!c.Casenumber}"/>
                        <apex:column value="{!c.Account_Name_web__c}"/>
                        <apex:column value="{!c.Status}"/>
                        <apex:column value="{!c.Dealer_Distributor__c}"/>
                        <apex:column value="{!c.CreatedDate}"/>
                        <apex:column value="{!c.Ownerid}"/>
                    </apex:pageblocktable>
                    <apex:panelGrid columns="2">
                    <apex:commandlink action="{!cs.previous}" status="status" rerender="myblock" rendered="{!hasPrevious}" value="< Previous"/>
                    <apex:commandlink action="{!cs.next}" status="status" rerender="myblock" rendered="{!hasNext}" value="Next >"/>
                    </apex:panelGrid>
                </apex:pageBlock>
            </apex:outputpanel>
        </div> 
    </apex:form>
</apex:page>