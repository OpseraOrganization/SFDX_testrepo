<apex:page cache="true" expires="10"  controller="AOGGetOneCase" showHeader="false" standardStylesheets="false">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>

<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="viewport" content="width=device-width; initial-scale=1.0" />
<meta name="HandheldFriendly" content="true"/>
<meta name="MobileOptimized" content="320"/>
<meta http-equiv="cleartype" content="on" />
<meta http-equiv="Cache-Control" content="no-cache" />
<meta name="format-detection" content="telephone=no" />

<title>AOG Dashboard Mobile</title>
<style type="text/css">
body {
    font-family: Verdana, Geneva, sans-serif;
    font-size:12px;
    background-color: #000;
    color: #FFF;
    margin: 0px;
    padding: 0px;
}
#fancybox-content a {
    color: #FFF;
    text-decoration: underline;
}
#fancybox-content tr th {
    background-image: url({!$Resource.bb_header});
    background-repeat: no-repeat;
    background-position: center top;
    text-align: center;
    vertical-align: bottom;
    height: 38;
    padding-bottom: 10px;
    padding-left: 0px;
    padding-right: 0px;
    padding-top: 50px;
}
#fancybox-content tr th.footer {
    background-image: none;
    padding-top: 10px;
}
#fancybox-content .star { 
    background-image: url({!$Resource.mid_star_sprite});
    float: left; 
    margin-top: 15px;
}
#fancybox-content .pop-up_header {
    height: 120px;
    display: block;
    text-align: left;
    padding-right: 50px;
    padding-left: 50px;
    padding-top: 20px;
    background-repeat: repeat-x;
    background-position: center bottom;
    width: auto;
}
#fancybox-content .pop-up_columns {
    float: left;
    margin-bottom: 20px;
    margin-top: 20px;
}
#fancybox-content .pop-up_columns td {
    vertical-align:top;
    padding-top: 3px;
    padding-bottom: 3px;
}
#fancybox-content .pop-up_columns tr:nth-child(odd) {
    background-color: #103052;
}
#fancybox-content .fields {
    color: #73A7E0;
    width: 30%;
}
#fancybox-content .pop-up_column.values {
    color: #FFF;
}
#fancybox-content td.fields {

}
#fancybox-content h2 {
    font-size: 24px;
}
#fancybox-content h4 {
    font-size: 16px;
    margin: 0px;
}
#fancybox-content h5 {
    margin: 0px;
}
#fancybox-content .star {
    background-repeat: no-repeat;
    height: 45px;
    width: 90px;
    background-position: 0px 0px;
}
#fancybox-content .star.blue.half {
    width: 60px;
    margin-right: 40px;
    background-position: -90px -90px;
}
#fancybox-content .star.one {
    background-position: 0px -45px;
}
#fancybox-content .star.blue {
    background-position: -90px 0px;
}
#fancybox-content .star.blue.one {
    width: 60px;
    margin-right: 40px;
    background-position: -90px -45px;
}
#fancybox-content .half {
    width: 60px;
    margin-right: 40px;
    background-position: 0px -90px;
}
#fancybox-content .two {
    width: 60px;
    margin-right: 40px;
}
#fancybox-content .three {
    width: 90px;
}
#fancybox-content .spex_label {
     color: #73A7E0;
     font-weight: bold;
}
#footerow {
    height: 35px;
    background-position: center top;
}
</style>
</head>
<body>
<table width="100%" border="0" align="center" cellpadding="5" cellspacing="0" id="fancybox-content">
  <tr>
    <th valign="bottom">
    <a name="top"></a>    
    <a href="#" onclick="javascript:history.back()">Return</a>
    </th>
  </tr>
  <tr>
    <td>

    <span style="float:left;">
    <h4>{!case_one.Account_Name__c}</h4>
    <h5>{!case_one.Aircraft_Type__r.Name}</h5>
    <apex:outputpanel styleClass="spex_label" layout="span" rendered="{!case_one.SPEX_Exchange__c}">SPEX</apex:outputpanel>
    </span>

                <apex:outputpanel layout="none" rendered="{!IF((case_one.Service_Level__c=='Superior' && case_one.X4_Hour_AOG__c=='24-hour' ),true,false)}" >    
                <div class="star three"></div>
                </apex:outputpanel>
                 
                <apex:outputpanel layout="none" rendered="{!IF((case_one.Service_Level__c=='Comprehensive' && case_one.X4_Hour_AOG__c=='24-hour' ),true,false)}" >
                 <div class="star two"></div>
                </apex:outputpanel>
                  
                <apex:outputpanel layout="none" rendered="{!IF((case_one.Service_Level__c=='Standard' && case_one.X4_Hour_AOG__c=='24-hour' ),true,false)}" >
                 <div class="star one"></div>
                </apex:outputpanel>
                     
                <apex:outputpanel layout="none" rendered="{!IF((case_one.Service_Level__c=='Superior' && case_one.X4_Hour_AOG__c=='4-hour' ),true,false)}" >
                 <div class="star blue three"></div> 
                </apex:outputpanel>
                
                <apex:outputpanel layout="none" rendered="{!IF((case_one.Service_Level__c=='Comprehensive' && case_one.X4_Hour_AOG__c=='4-hour' ),true,false)}" >
                 <div class="star blue two"></div>
                </apex:outputpanel>
                      
                <apex:outputpanel layout="none" rendered="{!IF((case_one.Service_Level__c=='Standard' && case_one.X4_Hour_AOG__c=='4-hour' ),true,false)}" >
                 <div class="star blue one"></div>
                </apex:outputpanel>


        <table width="100%" border="0" cellpadding="0" cellspacing="0" class="pop-up_columns">
          <tr>
            <td class="fields">Status:</td>
            <td>{!IF(case_one.Sub_Status__c!=null,case_one.Sub_Status__c,"New AOG - TBD")}</td>
          </tr>
          <tr>
            <td class="fields">Aircraft Location:</td>
            <td>{!case_one.Aircraft_Location__c}</td>
          </tr>
          <tr>
            <td class="fields">Next Flight:</td>
            <td>{!case_one.Next_Flight_Out_Date__c}</td>
          </tr>
          <tr>
            <td class="fields">Part Number:</td>
            <td>{!case_one.Product_Part__c} - {!case_one.Product_Name__c}</td>
          </tr>
          <tr>
            <td class="fields">Ship From:</td>
            <td>{!case_one.Hardware_Ship_From_Address__c}</td>
          </tr>
          <!--
          <tr>
            <td class="fields">AOG OTTR Miss Reason:</td>
            <td>{!IF((case_one.AOG_OTTR_Miss_Reason__c==''),'Material Shortage',case_one.AOG_OTTR_Miss_Reason__c)}</td>
          </tr>
          <tr>
            <td class="fields">AOG OTTR Miss Detail:</td>
            <td>{!IF((case_one.AOG_OTTR_Miss_Detail__c==''),'WOR (SPEX)',case_one.AOG_OTTR_Miss_Detail__c)}</td>
          </tr>
          -->
          <tr>
            <td class="fields">Market:</td>
            <td>{!case_one.Market__c}</td>
          </tr>
          <tr>
            <td class="fields">SBU:</td>
            <td>{!case_one.SBU__c}</td>
          </tr>
          <tr>
            <td class="fields">CBT Team:</td>
            <td>{!case_one.CBT_Team__c}</td>
          </tr>
          <tr>
            <td class="fields">Contract Type:</td>
            <td>{!case_one.Contract_Type__c}</td>
          </tr>
          <!--
          <tr>
            <td class="fields">Order Entered:</td>
            <td>{!case_one.Order_Entered__c}</td>
          </tr>
          <tr>
            <td class="fields">Customer Request Date:</td>
            <td>{!case_one.Customer_Request_Date__c}</td>
          </tr>
          -->
          <tr>
            <td class="fields">Case Owner:</td>
            <td>{!case_one.OwnerName__c}</td>
          </tr>
        </table>

  </td>
  </tr>
  
  <tr>
  <th class="footer"><a href="#" onclick="javascript:history.back()">Return</a> | <a href="#top" class="button">Go to Top</a></th>
  </tr>
</table>
</body>
</html>
</apex:page>