<script>
var userAgent = navigator.userAgent;
function resizeWindPartSupport()  {      
    if (userAgent.match(/(iPhone|iPod|Android|BlackBerry|IEMobile|Windows Phone)/g)) {    
        $("#parts-page .aog-content").height($("html").height() - $(".ui-header").height()- $(".ui-footer").height() - 40);
    }
    else if (userAgent.match(/(iPad)/g)) { 
        $("#parts-page .aog-content").height($("html").height() - $(".ui-header").height()- $(".ui-footer").height() - 80);
    }
    else {   
        $("#parts-page .aog-content").height($("html").height()- $(".ui-header").height()- $(".ui-footer").height() - 60);
    }
}
