<script>

function resizeWindHelp() {
    var userAgent = navigator.userAgent;
    if (userAgent.match(/(iPhone|iPod|Android|BlackBerry|IEMobile|Windows Phone)/g)) {    
        $("#help-page .aog-content").height($("html").height() - $(".ui-header").height()- $(".ui-footer").height() - 40);
    }
    else if (userAgent.match(/(iPad)/g)) { 
        $("#help-page .aog-content").height($("html").height() - $(".ui-header").height()- $(".ui-footer").height() - 80);
    }
    else {   
        $("#help-page .aog-content").height($("html").height()- $(".ui-header").height()- $(".ui-footer").height() - 60);
    }
}

$( window ).on( "pageshow", function( event ) {
   $(".bga-loader").hide(); // To Hide loader
    resizeWindHelp();
});
    

$( window ).on( "orientationchange", function( event ) {
    resizeWindHelp();
});
    
  
$(document).ready(function() {

    resizeWindHelp();
    
    $(window).resize(function(){
        resizeWindHelp();
    });

    if($.cookie("the_cntyval") == 'United States') {
        $("#tso-number-lbl").append("<apex:outputText value="{!Tech_TOC_USNo}"></apex:outputText>&nbsp;");
        $("#tso-tech-phone-no").attr('href','tel:{!Tech_TOC_USNo}');
        $("#tso-tech-phone-no").append("<apex:outputText value="{!Tech_TOC_USNo}"></apex:outputText>");
    }
    else {
        $("#tso-number-lbl").append("<apex:outputText value="{!Tech_TOC_IntlNo}"></apex:outputText>&nbsp;");
        $("#tso-tech-phone-no").attr('href','tel:{!Tech_TOC_IntlNo}');
        $("#tso-tech-phone-no").append("<apex:outputText value="{!Tech_TOC_IntlNo}"></apex:outputText>");
    }
  
    // add to home screen -- addendum
   if (("standalone" in window.navigator) && window.navigator.standalone) {
  // For iOS Apps
  $("a").on("click", function(e){
    var new_location = $(this).attr("href");
    if (new_location != undefined && new_location.substr(0, 1) != "#" && new_location!='' && $(this).attr("data-method") == undefined){
      e.preventDefault();
      window.location = new_location;
    }
  });
}

    
});
 
</script>
