            <div class="process" id="cases">
                <div class="step step-1 review" data-head-cs-icon="{!URLFOR($Asset.SLDS, 'assets/icons/standard-sprite/svg/symbols.svg#case')}" data-head-h1="{!$Label.ag_caseManagement}" data-show-install="casesTemplate">
