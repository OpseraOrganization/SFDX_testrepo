                <li class="slds-wizard__item">
                    <a href="javascript:void(0);" class="slds-wizard__link">
                        <span class="slds-wizard__marker"></span>
                        <span class="slds-wizard__label slds-text-title--caps slds-truncate" title="Navigation">{!$Label.ag_complete}</span>
                    </a>
                </li>
            </ol>
            <span class="slds-wizard__progress">
                <span class="slds-wizard__progress-bar" style="width:0%"></span>
            </span>
        </div>
        <div class="workflow">
            <div class="process" id="start">
                <div class="step" data-head-icon="checkin" data-head-subtitle="{!$Label.ag_Quickstart}" data-head-h1="{!$Label.ag_GettingStarted}">
                    <div class="header">
                        <p class="title">{!$Label.ag_howDoYouWantToSetupActionGrid}</p>
                    </div>
                    <ol class="selection-grid">
                        <li class="tile">
