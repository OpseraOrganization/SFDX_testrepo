                                    <svg aria-hidden="true" class="slds-icon" style="fill:#2dad25">
                                        <use xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/standard-sprite/svg/symbols.svg#account')}"></use>
                                    </svg>
                                </span>
                                <span class="title">{!$Label.ag_CPQ}</span>
