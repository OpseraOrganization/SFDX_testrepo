                                    <svg aria-hidden="true" class="slds-icon" style="fill:#53CDB8">
                                        <use xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/custom-sprite/svg/symbols.svg#custom11')}"></use>
                                    </svg>
                                </span>
                                <span class="title">{!$Label.ag_sales}</span>
                                <span class="description">{!$Label.ag_keepTrackOfLeadsAndRelatedOpportunitiesAccountsTasks}</span>
