                    </ol>
                </div>
            </div>
            <div class="process" id="sales">
                <div class="step step-1 review" data-head-cs-icon="{!URLFOR($Asset.SLDS, 'assets/icons/custom-sprite/svg/symbols.svg#custom11')}" data-head-h1="{!$Label.ag_sales}" data-show-install="salesTemplate">
