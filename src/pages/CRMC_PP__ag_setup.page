<apex:page showHeader="true" sidebar="false" controller="CRMC_PP.AGComponentController" title="Conga Grid Setup" docType="html-4.01-strict">
    <head>
        <apex:slds />
        <apex:stylesheet value="{!URLFOR($Resource.CRMC_PP__GridResources, '/styles/kendo-slds.css')}"/>
        <apex:stylesheet value="{!URLFOR($Resource.CRMC_PP__GridResources, '/styles/core.min.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.CRMC_PP__GridResources, '/styles/quickstart.min.css')}"/>

        <apex:includeScript value="{! URLFOR($Resource.kendoui, '/js/jquery.min.js')}" />
        <apex:includeScript value="../../soap/ajax/50.0/connection.js"/>
        <!-- <apex:includeScript value="{!$Resource.kendodebug}" />  -->
        <apex:includeScript value="{! URLFOR($Resource.kendoui, '/js/kendo.web.min.js')}" />

        <!-- Allows slds dynamic svgs to work in Edge LEX and IE 11 -->
        <apex:includeScript value="{!URLFOR($Resource.CRMC_PP__GridResources, '/Libs/svg4everybody.min.js')}"/>

        <!-- Localization Utility -->
        <c:Localization />

        <apex:includeScript value="{!$Resource.CRMC_PP__Admin}"/>
        <apex:includeScript value="{! $Resource.Conga_Grid_Instrumentation }" />

        <script type="text/javascript">
            try {
                svg4everybody();
            } catch (error) {
                // Swallow error, not user impacting
            }

            sforce.connection.sessionId = "{!JSENCODE($Api.Session_ID)}";
            sforce.connection.client ="CRMCulture/ActionGrid/";

            var urls = {
                salesTemplate: "{!$Resource.Sales_Console_Template}",
                casesTemplate: "{!$Resource.Case_Console_Template}",
                accountsTemplate: "{!$Resource.Account_Console_Template}",
                adminTemplate: "{!$Resource.Admin_Console_Grid}",
                cpqTemplate: "{!$Resource.CPQ_Console_Template}",
                congaContractsTemplate: "{! $Resource.Conga_Contracts_Console_Template }"
            };

            crmc.require(['GridConstants'], function(constants) {
                constants.SLDS_URL = '/apexpages/slds/latest';
            });

            crmc.require(["QuickStart"], function(quickStart){
                var sessionId = "{!$Api.Session_ID}";

                $(function(){
                    quickStart.init(sessionId, urls);
                });
            });

            pendo.initialize({
                visitor: {
                    id: '{! JSENCODE($Organization.Id) }' + '-' +'{! JSENCODE($User.Id) }'
                },
                account: {
                    id: '{! JSENCODE($Organization.Id) }',
                    name: '{! JSENCODE($Organization.Name) }'
                }
            });
        </script>
    </head>

<!-- Notification Banner -->
<c:NotifyBanner ParentPage="ag-setup" />

<div class="ActionGrid Wizard slds-scope" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
    <div class="container header slds-page-header slds-m-around--large" role="banner">
        <div class="slds-grid">
            <div class="slds-col slds-has-flexi-truncate">
                <div class="slds-media slds-no-space slds-grow">
                    <div class="slds-media__figure">
                        <span class="icon k-i-checkin"></span>
                        <span class="slds-icon_container hidden">
                            <svg aria-hidden="true" class="slds-icon">
                                <use xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/utility-sprite/svg/symbols.svg#checkin')}"></use>
                            </svg>
                        </span>
                    </div>
                    <div class="slds-media__body">
                        <p class="subtext slds-text-title--caps slds-line-height--reset"> {!$Label.ag_Quickstart}</p>
                        <h1 class="AG-h1 slds-m-right--small slds-align-middle slds-truncate">{!$Label.ag_GettingStarted}</h1>
                    </div>
                </div>
            </div>
            <div class="slds-col slds-no-flex slds-grid slds-align-top">
                <div class="actions">
                    <ol>
                        <li class="item hidden">
                            <a href="javascript:void(0)" class="retractButton" id="retreat"> {!$Label.ag_StartOver}</a>
                        </li>
                        <li class="item active-submit hidden">
                            <button class="CTA"></button>
                            <span class="status"> {!$Label.ag_Installing}</span>
                        </li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
