        //INC000006348765 - Countdown for IES Starts
        $(document).ready(function(){
            
            var targetDate = '{!$Label.A350_EIS_Target}';
            if(targetDate != '' && targetDate != undefined){
                var eisDate = new Date(targetDate);
                var toadysDate = new Date();
                
                //Formating the date (Ex: November 18, 2014)
                var m_names = new Array("January", "February", "March", 
                "April", "May", "June", "July", "August", "September", 
                "October", "November", "December");
                
                var d = eisDate;
                var eis_date = d.getDate();
                var eis_month = d.getMonth();
                var eis_year = d.getFullYear();
                
                var congratsMsg = '<span style="font-size:20px">Congratulations!</span><span style="display:inline;color: #999999;"> the A350 aircraft has entered into service on '+ m_names[eis_month] + ' ' + eis_date +'</span>';
                var diff = getDaysDifference(eisDate, toadysDate);
                $('#eisDate').html('('+ targetDate +')');
                
                if(diff >= 0){
                    $('#eisDaysLeft').html(diff);
                    if(diff == 0){
                        $('#idCongratsMsg').html(congratsMsg);
                    }
                }else{
                    $('#eisDaysLeft').html("0");
                    $('#idCongratsMsg').html(congratsMsg);
                }
            }else {
                $('#eisCountdown').hide();
            }    
        });
        
        
        
        //Calculate the difference between the days
        function getDaysDifference(eisDate, todaysDate) {

            // The number of milliseconds in one day
            var ONE_DAY = 1000 * 60 * 60 * 24;
        
            var todaysFinalDate = new Date(todaysDate.getFullYear(),todaysDate.getMonth(),todaysDate.getDate());
            //alert(todaysFinalDate);
            // Convert both dates to milliseconds
            var eisDate_ms = eisDate.getTime();
            var todaysDate_ms = todaysFinalDate.getTime();
        
            // Calculate the difference in milliseconds
            var difference_ms = eisDate_ms - todaysDate_ms;
            //alert(difference_ms/ONE_DAY);
            // Convert back to days and return
            return Math.ceil(difference_ms/ONE_DAY);
        
        }
