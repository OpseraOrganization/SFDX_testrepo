  <script type="text/javascript">

    $(document).ready(function() {      
         
      $('.dashboard-thumbnail span[rel=tooltip]').tooltip();

      $('.jclock').jclock();
      
      setTicker = function(bid, change, storeStock){
        storeStock = (typeof storeStock === "undefined") ? true : storeStock;
        
        changeIcon = (change>0?"icon-arrow-up":"icon-arrow-down");
        $(".headers .stock").html("<div class='change'>HON:"+bid+"</div><i class='icon-white "+changeIcon+"'></i>"+change);
        
        if(storeStock){
            $.cookie('bid',bid);
            $.cookie('change',change);
        }
      }

      getStockYAH = function(){
        $.ajax({
          url: 'http://query.yahooapis.com/v1/public/yql?q=select%20BidRealtime, Change%20from%20yahoo.finance.quotes%20where%20symbol%20in%20%28%22HON%22%29%0A%09%09&env=http%3A%2F%2Fdatatables.org%2Falltables.env&format=json',
          dataType: "json",
          success: function(data) {
              if( data.query.results != null && data.query.results.quote.BidRealtime != null ){
                setTicker(data.query.results.quote.BidRealtime,data.query.results.quote.Change);
              }
              else{
                $(".headers .stock").html("");    //N/A
                $(getStockGOO);
              }
          },
          error: function(data) {
              $(".headers .stock").html("");      //Error
              $(getStockGOO);
          } 
        });
      };
      
      getStockGOO = function(){
        $.ajax({
          url: 'http://www.google.com/finance/info?q=HON',
          dataType: "jsonp",
          jsonpCallback: "setGOOvalu",
          timeout: 10000,
          error: function(){
            setTicker( $.cookie('bid'), $.cookie('change'), false );
          }
        });
      };
      
      setGOOvalu = function(results){
          if( results[0].l != null ){
            setTicker(results[0].l,results[0].c);
          }
          else{
            $(".headers .stock").html("");    //N/A
            setTicker( $.cookie('bid'), $.cookie('change'), false );
          }
      }
      
      $(getStockYAH);
      
        //INC000006348765 - Countdown for IES Starts
        
        var targetDate = '{!$Label.A350_EIS_Target}';
        if(targetDate != '' && targetDate != undefined){
            var eisDate = new Date(targetDate);
            var toadysDate = new Date();
            
            var diff = getDaysDifference(eisDate, toadysDate);
            
            //Formating the date (Ex: November 18, 2014)
            var m_names = new Array("January", "February", "March", 
            "April", "May", "June", "July", "August", "September", 
            "October", "November", "December");
            
            var d = eisDate;
            var eis_date = d.getDate();
            var eis_month = d.getMonth();
            var eis_year = d.getFullYear();
            
            
            
            if(diff > 0){
                $('h4[name="eisDaysLeft"]').html(diff + ' Days to Goal');
                $('font[name="eisDate"]').html('<b> ' + m_names[eis_month] + ' ' + eis_date + ',' + ' ' + eis_year + '</b>');
            }else{
                $('h4[name="eisDaysLeft"]').html('Congratulations');
                $('font[name="eisDate"]').attr('style','');
                $('font[name="eisDate"]').html('the A350 aircraft has entered into service on ' + m_names[eis_month] + ' ' + eis_date);
            }
        }else {
            $('#eisCountdown').hide();
        }   
        
        //INC000006348765 - Countdown for IES Ends 

    }); 

    //INC000006348765 - Countdown for IES Starts
    //Calculate the difference between the days
    function getDaysDifference(eisDate, todaysDate) {

        // The number of milliseconds in one day
        var ONE_DAY = 1000 * 60 * 60 * 24;
        var todaysFinalDate = new Date(todaysDate.getFullYear(),todaysDate.getMonth(),todaysDate.getDate());
        
        // Convert both dates to milliseconds
        var eisDate_ms = eisDate.getTime();
        var todaysDate_ms = todaysFinalDate.getTime();
    
        // Calculate the difference in milliseconds
        var difference_ms = eisDate_ms - todaysDate_ms;
        
        // Convert back to days and return
        return Math.ceil(difference_ms/ONE_DAY);
    
    }
    //INC000006348765 - Countdown for IES Ends
    
