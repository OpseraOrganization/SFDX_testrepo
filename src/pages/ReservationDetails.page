<apex:page standardController="Reservation__c" showHeader="true" extensions="Reservationdetailcls" cache="true"> 
   <apex:form >
   <!-- to display reservation name and printable view link--->
   <div class="bPageTitle">
        <div class="ptBody secondaryPalette">
            <div class="content">
               <img src="/s.gif" alt="Reservation"  class="pageTitleIcon" title="Reservation"/>
               <h1 class="pageType">Reservation<span  class="titleSeparatingColon">:</span></h1>
               <h2 class="pageDescription">{!res.Name}</h2>
               <div class="blank">&nbsp;</div>
            </div>
            <div class="links">
               <a href="javascript:printWin(%27/a0Pa000000D21az/p?retURL=%2Fa0Pa000000D21azEAB%27)" class="configLinks" title="Printable View (New Window)">Printable View</a>
            </div>
        </div>
       <div class="ptBreadcrumb"></div>
    </div>
    <script type="text/javascript">if (window.picklist) {picklist.initAll();}</script>
    <script type="text/javascript"></script>
    <!-- display reservation details---->
   <apex:pageBlock id="editUserDetail" title="Reservations">       
        <apex:pageBlockSection columns="2">            
            <apex:outputField value="{!res.Name}"/>
            <apex:outputField value="{!res.Course_Name__c}"/>                   
            <apex:outputField value="{!res.Legacy_Confirmation_Key__c}"/>
            <apex:outputField value="{!res.Class_Name__c}"/> 
            <apex:outputField value="{!res.Student__c}"/>
            <apex:outputField value="{!res.Entitlement__c}"/> 
            <apex:outputField value="{!res.Email__c}"/>
            <apex:outputField value="{!res.Reservation_Type__c}"/> 
            <apex:outputField value="{!res.Reservation_Organizer__c}"/>
            <apex:outputField value="{!res.Reservation_Status__c}"/> 
            <apex:outputField value="{!res.Organizer_Email__c}"/>
            <apex:outputField value="{!res.Start_Date__c}"/> 
            <apex:outputField value="{!res.Attended__c}"/>
            <apex:outputField value="{!res.End_Date__c}"/> 
            <apex:outputField value="{!res.Legacy_Id__c}"/>
            <apex:outputField value="{!res.Start_Time__c}"/> 
            <apex:outputField value="{!res.Reservation_Name__c}"/>
            <apex:outputField value="{!res.End_Time__c}"/> 
            <apex:outputField value="{!res.createdbyid}">,
            <apex:outputField value="{!res.CreatedDate}"></apex:outputField>
            </apex:outputField>           
            <apex:outputField value="{!res.lastmodifiedbyid}">,
              <apex:outputField value="{!res.LastModifiedDate}"></apex:outputField>
            </apex:outputField>
        </apex:pageBlockSection>            
    </apex:pageBlock>
    </apex:form>
     <apex:relatedList list="NotesAndAttachments"/>
</apex:page>