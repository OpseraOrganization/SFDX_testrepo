<apex:page standardController="Case" extensions="OwnerChange">
    <apex:includeScript value="/soap/ajax/21.0/connection.js"/>
    <apex:includeScript value="/soap/ajax/21.0/apex.js"/>
    <apex:outputPanel id="scr" rendered="{!callwindow}">
        <script>   
            window.top.location.href ="https://c.na19.visual.force.com/apex/Changelink1?Id={!Case.id}";
        </script>     
    </apex:outputPanel>
    <apex:outputPanel id="scr1" rendered="{!callwindow1}">
        <script>   
            window.top.location.href ="https://na19.salesforce.com/{!Case.id}/a?retURL=%2F{!Case.id}";
        </script>     
    </apex:outputPanel>
    <apex:form >
    <apex:outputField value="{!Case.OwnerId}" label="Case Owner"/>
    <apex:commandLink action="{!popup}">[Change]</apex:commandLink>
    </apex:form>
</apex:page>