<apex:page standardController="Case" extensions="SkillRelatedListExtension">
<apex:includeScript value="/support/console/22.0/integration.js"/>
    <script type="text/javascript">
        function testOpenPrimaryTab(cid, cn) {
           var url = location.href;  // entire url including querystring - also: window.location.href;
            var baseURL = url.substring(0, url.indexOf('/', 14));
            console.log('Base URL = ',baseURL);
            console.log('cid = ',cid);
            var baseURL1 = baseURL+'/'+cid;
            console.log('url = ',baseURL1);
            //Open a new primary tab with the salesforce.com home page in it
            sforce.console.openPrimaryTab(null, baseURL1, true,
                cn, openSuccess, 'salesforceTab');
        }
         
        var openSuccess = function openSuccess(result) {
            //Report whether opening the new tab was successful
            if (result.success == true) {
                //alert('Primary tab successfully opened');
            } else {
                //alert('Primary tab cannot be opened');
            }
        };
  </script>

 <apex:form >
 <apex:pageMessages />

 <apex:pageblock id="CustomList" title="Skills">
 <apex:pageblocktable value="{!sk}" var="o" rendered="{!NOT(ISNULL(sk))}">
  
 <apex:column headerValue="Name"><a HREF="#" onClick="testOpenPrimaryTab('{!o.contact__r.id}','{!o.contact__r.name}');return false">{!o.contact__r.name}</a></apex:column>
 <apex:column value="{!o.Expertise__c}"/>
 <apex:column value="{!o.Tier_4_Teams__c}"/>
 <apex:column value="{!o.Region__c}"/>
 <apex:column value="{!o.Skill_Type__c}"/>
 <apex:column value="{!o.Supported_Products__r.name }"/>    
 </apex:pageblocktable>
 </apex:pageblock>
 </apex:form>
</apex:page>