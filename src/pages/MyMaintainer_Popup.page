<apex:page standardController="Case" extensions="MyMaintainer_PopupClass">
<script type="text/javascript">
    var requests = [];
</script>
    <apex:repeat value="{!cs}" var="request"> 
        <script> 
            requests.push('{!request}'); 
        </script> 
    </apex:repeat>
    <script>
        var sub = '{!Case.Subject}';
        var subfound = sub.search('Maintenance Advisor');
        window.document.onload = new function(){
        if({!Case.Case_Record_Type__c=='WEB Portal Registration'} && requests[0]==null && subfound!=-1){
            alert('Please fill Maintenance Advisor Role field for the user');
        }
    }
    </script>
</apex:page>