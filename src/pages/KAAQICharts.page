<apex:page controller="controller_for_KA_AQI_Report" sidebar="false">
 <apex:chart height="400" width="50%" data="{!Data}">
 <apex:legend position="right"/>
          <apex:axis type="Numeric" position="left" fields="KAcount,AQIcount" 
            title="Count" grid="true" />
        
          <apex:axis type="Category" position="bottom" fields="Cname" 
            title="Coach Name">
            <apex:chartLabel rotate="270"/>
            </apex:axis>
        <apex:barSeries orientation="vertical" axis="left" 
            xField="Cname" yField="KAcount,AQIcount">
            <apex:chartTips height="20" width="120"/>
            
        </apex:barSeries>
            
    </apex:chart>
    <apex:outputLink ></apex:outputLink>
    <apex:outputlink title="Link to view report details"  value="/apex/KAAQIReport" target="_blank" ><apex:outputtext value="Link to view report details"/></apex:outputlink>
</apex:page>