<apex:page controller="CustomResOrganizerLookupController" title="Search" showHeader="false" sideBar="false" tabStyle="Reservation__c" id="pg" cache="true">
  <apex:form >
      <div class="bPageTitle">
      <div class="ptBody secondaryPalette">
     <div class="content"><img src="/s.gif" alt=""  class="pageTitleIcon" title=""/><h1>Lookup</h1></div>
     </div></div>
     <apex:outputPanel id="page" layout="block" style="margin:5px;padding:10px;padding-top:2px;">
        <apex:actionRegion >  
          <apex:outputPanel id="top" layout="block" style="margin:5px;padding:10px;padding-top:2px;">
            <apex:outputLabel value="Search" style="font-weight:Bold;padding-right:10px;" for="txtSearch"/>
            <apex:inputText id="txtSearch" value="{!searchString}" />
              <span style="padding-left:5px"><apex:commandButton id="btnGo" value="Go" action="{!Search}" rerender="searchResults"></apex:commandButton></span>
          </apex:outputPanel>
 
          <apex:outputPanel id="pnlSearchResults" style="margin:10px;height:350px;overflow-Y:auto;overflow-X:auto;" layout="block">
            <apex:pageBlock id="searchResults" title="Contacts">                
                <center>
                <div class="pbBody" id="Contact_body">
                  <div class="relatedListsearchHeader">
                  <div class="pSearchShowMore">
                      <apex:commandLink rendered="{!enablePrevious}" action="{!previous}"><img src="/img/search_prevarrow.gif" alt="No Previous Page" width="5" height="8"  title="No Previous Page" /></apex:commandLink>&nbsp;&nbsp;&nbsp;
                      <apex:commandLink action="{!previous}" rendered="{!enablePrevious}">Previous Page</apex:commandLink>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                      <apex:commandLink action="{!next}" rendered="{!enableNext}"><span  class="pShowMore">Next Page</span></apex:commandLink>&nbsp;&nbsp;&nbsp;
                      <apex:commandLink action="{!next}" rendered="{!enableNext}"><img src="/img/search_nextarrow.gif" alt="Next Page" width="5" height="8" title="Next Page" /></apex:commandLink>
                  </div>
                  </div>
                  </div>
                  </center>
                <apex:pageBlockTable value="{!results}" var="a" id="tblResults">
                <apex:column >
                  <apex:facet name="header">
                    <apex:outputPanel >Name</apex:outputPanel>
                  </apex:facet>
                     <apex:outputLink value="javascript:top.window.opener.lookupPick2('{!FormTag}','{!TextBox}_lkid','{!TextBox}','{!a.Id}','{!a.Name}', false)" rendered="{!NOT(ISNULL(a.Id))}">{!a.Name}</apex:outputLink>                       
                </apex:column>
                <apex:column value="{!a.Contact_Status__c}"/>
                <apex:column value="{!a.Accountid}"/>
                <apex:column value="{!a.Title}"/>
                <apex:column value="{!a.Phone_1__c}"/>
                <apex:column value="{!a.email}"/>
                <apex:column value="{!a.City_Name__c}"/>
                <apex:column value="{!a.State_Code__c}"/>
                <apex:column value="{!a.Country_Name__c}"/>
                 
              </apex:pageBlockTable>
             
            </apex:pageBlock>
          </apex:outputPanel>
        </apex:actionRegion>    
  </apex:outputPanel>
  </apex:form>
</apex:page>