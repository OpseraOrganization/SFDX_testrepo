<apex:page controller="DSPC_Campaign_List_controller">
    <apex:image id="theImage" value="{!$Resource.DSPC_Campaign_Home}"/>
    <apex:form id="formOne">
    <div style="position:relative;">
    <apex:outputpanel >
        <apex:actionstatus id="status">
            <apex:facet name="start">
                <div class="waitingSearchDiv" id="el_loading" style="background-color: #FFFFFF; height: 100%; width:100%; opacity: 0.95">
                    <div class="waitingHolder" style="top: 74.2px; width: 100%; background-color: #FFFFFF;">
                        <img class="waitingImage" src="{!$Resource.DSPC_Loading}" title="Please Wait..."/>
                    </div>
                </div>
            </apex:facet>
        </apex:actionstatus>
         <apex:pageBlock id="myblock">
             <apex:pageblocktable id="tableOne" value="{!AllSearchcams}" var="c">
                 <apex:column headerValue="Campaign Name"><a href="/dspc/{!c.id}">{!c.name}</a></apex:column>
                 <apex:column value="{!c.IsActive}"/>
                 <apex:column value="{!c.Status}"/>
                 <apex:column value="{!c.BudgetedCost}"/>
                 <apex:column value="{!c.ActualCost}"/>
                 <apex:column value="{!c.CampaignPrimarySBU__c}"/> 
             </apex:pageblocktable>
             <apex:panelGrid columns="2">
    <apex:commandlink action="{!cam.previous}" status="status" rerender="myblock" rendered="{!hasPrevious}" value="< Previous"/>
    <apex:commandlink action="{!cam.next}" status="status" rerender="myblock" rendered="{!hasNext}" value="Next >"/>
    </apex:panelGrid>
         </apex:pageBlock>
     </apex:outputpanel>
     </div>
     </apex:form>
</apex:page>