<script>

function resizeWindAOG() {
    var userAgent = navigator.userAgent;
    if (userAgent.match(/(iPhone|iPod|Android|BlackBerry|IEMobile|Windows Phone)/g)) {    
        $("#aog-page .aog-content").height($("html").height() - $(".ui-header").height()- $(".ui-footer").height() - 40);
    }
    else if (userAgent.match(/(iPad)/g)) { 
        $("#aog-page .aog-content").height($("html").height() - $(".ui-header").height()- $(".ui-footer").height() - 80);
    }
    else {   
        $("#aog-page .aog-content").height($("html").height()- $(".ui-header").height()- $(".ui-footer").height() - 60);
    }
}


$(document).bind("pageshow", function (event, data) {
    $(".bga-loader").hide();
    /*var $this = $( this ),
        theme = $this.jqmData( "theme" ) || $.mobile.loader.prototype.options.theme,
        msgText = $this.jqmData( "msgtext" ) || $.mobile.loader.prototype.options.text,
        textVisible = $this.jqmData( "textvisible" ) || $.mobile.loader.prototype.options.textVisible,
        textonly = !!$this.jqmData( "textonly" );
        html = $this.jqmData( "html" ) || "";*/
    
    if($.cookie("the_cntyval") == 'United States') {    
        $("#aog-phone-no").attr('href','tel:{!AOG_USNo}');
        $("#aog-phone-no").append("<apex:outputText value="{!AOG_USNo}"></apex:outputText>");
        $("#aog-phone-lbl").append("<apex:outputText value="{!TollFree1_USNo}"></apex:outputText> Option 4");
    }
    else {
        $("#aog-phone-no").attr('href','tel:{!AOG_IntlNo}');
        $("#aog-phone-no").append("<apex:outputText value="{!AOG_IntlNo}"></apex:outputText>");
        $("#aog-phone-lbl").append("<apex:outputText value="{!TollFree1_IntlNo}"></apex:outputText> Option 4");
    }
  
});

$( window ).on( "pageshow", function( event ) {
    resizeWindAOG();
});
    

$( window ).on( "orientationchange", function( event ) {
    resizeWindAOG();
});
    
$(document).ready(function() {

    resizeWindAOG();
   
    $(window).resize(function(){
        resizeWindAOG();
    });  
    if (("standalone" in window.navigator) && window.navigator.standalone) {
    // For iOS Apps
    $('a').on('click', function(e){
      e.preventDefault();
      var new_location = $(this).attr('href');
      if (new_location != undefined && new_location.substr(0, 1) != '#' && $(this).attr('data-method') == undefined){
        window.location = new_location;
      }
    });
  }
});
function getContactPage(Id)
{
    if((navigator.userAgent.toLowerCase().match(/android/) || navigator.userAgent.toLowerCase().match(/iphone/) || navigator.userAgent.toLowerCase().match(/ipad/)) && !SFHybridApp.deviceIsOnline()){
         $.mobile.changePage("#offline-form-aog-page");
     }
     else
     { 
        var myhref = document.getElementById(Id);
        myhref.setAttribute('href', "/bgadir/BGAMOB_Custom_Webtocase");
     }
}
</script>
