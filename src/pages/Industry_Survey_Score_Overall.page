<apex:page standardcontroller="Account" extensions="Industry_Survey_Score_Overall_Cls" sidebar="false">
   
    <apex:form >
         <div style="position:relative;">
            <apex:outputpanel >
                <apex:actionstatus id="status">
                    <apex:facet name="start">
                        <div class="waitingSearchDiv" id="el_loading" style="background-color: #FFFFFF; height: 100%; width:100%; opacity: 0.95">
                            <div class="waitingHolder" style="top: 10px; width: 700px; height:120px; background-color: #FFFFFF; border:0px;">
                                <img class="waitingImage" src="{!$Resource.DSPC_Loading}" title="Please Wait..."/>
                            </div>
                        </div>
                    </apex:facet>
                </apex:actionstatus>
          
       </apex:outputpanel>
       
       
            <apex:panelGrid columns="2">
               <apex:outputLabel style="font-weight: 600"  value="Industry Survey Score Overall (Current Calendar Year)"></apex:outputLabel>                 
               <apex:image id="currentYR" height="30" width="100" url="{!IF(OR(roundedvalueCYR==0,roundedvalueCYR==1),'https://na19.salesforce.com/servlet/servlet.FileDownload?file=01513000003A2bf',IF(OR(roundedvalueCYR==3,roundedvalueCYR==4),'https://na19.salesforce.com/servlet/servlet.FileDownload?file=01513000003A2ba',IF(roundedvalueCYR==2,'https://na19.salesforce.com/servlet/servlet.FileDownload?file=01513000003A2bk','')))}"/>
               <apex:outputLabel style="font-weight: 600" value="Industry Survey Score Overall (Previous Calendar Year)"></apex:outputLabel>                 
               <apex:image id="previousYR" height="30" width="100" url="{!IF(OR(roundedvaluePYR==0,roundedvalueCYR==1),'https://na19.salesforce.com/servlet/servlet.FileDownload?file=01513000003A2bf',IF(OR(roundedvalueCYR==3,roundedvalueCYR==4),'https://na19.salesforce.com/servlet/servlet.FileDownload?file=01513000003A2ba',IF(roundedvalueCYR==2,'https://na19.salesforce.com/servlet/servlet.FileDownload?file=01513000003A2bk','')))}"/>
            </apex:panelGrid> 
       
     </div>  
   </apex:form>    
</apex:page>