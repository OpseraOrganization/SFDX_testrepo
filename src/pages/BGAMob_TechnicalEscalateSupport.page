        function showPosition(position)
        {  
            geocoder = new google.maps.Geocoder();
            var lat=parseFloat(position.coords.latitude);
            var lng=parseFloat(position.coords.longitude);
            var latlng = new google.maps.LatLng(lat,lng);
            geocoder.geocode({'latLng': latlng}, function(results, status) {
                if (status == google.maps.GeocoderStatus.OK) {
                    if (results[0]) {
                        var arrAddr = results[0].address_components;
                        var country;
                        $.each(arrAddr, function(i, address_component) {
                            if(address_component.types[0] == "country") {
                                country = address_component.long_name.toString();               
                                $.cookie('the_cntyval', country, { expires: 1, path: '/' });
                            }
                            if(address_component.types[0] == "administrative_area_level_1") {
                                state= address_component.long_name.toString();
                            //alert(state);
                                $.cookie('the_stateval', state, { expires: 1, path: '/' });
                            }
                        });
                        alert('country'+country);
                    }
                }     
            }); 
        }
