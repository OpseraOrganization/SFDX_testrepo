<apex:page cache="true" expires="10"  controller="RemovalsReport" showHeader="false" standardStylesheets="false">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>

<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="viewport" content="width=device-width; initial-scale=1.0" />
<meta name="HandheldFriendly" content="true"/>
<meta name="MobileOptimized" content="320"/>
<meta http-equiv="cleartype" content="on" />
<meta http-equiv="Cache-Control" content="no-cache" />
<meta name="format-detection" content="telephone=no" />

<title>A350 Removals Dashboard Mobile</title>
<style type="text/css">
body {
    font-family: Verdana, Geneva, sans-serif;
    font-size:12px;
    background-color: #000;
    color: #FFF;
    margin: 0px;
    padding: 0px;
}
a, a:hover{
    color: #FFF;
    text-decoration: none;
}
.totals span {
    color: #0084CA;
}
#datatable tr th {
    background-image: url({!$Resource.bb_header});
    background-repeat: no-repeat;
    background-position: center top;
    text-align: center;
    vertical-align: bottom;
    height: 38;
    padding-bottom: 10px;
    padding-left: 0px;
    padding-right: 0px;
    padding-top: 50px;
}
#datatable tr th.footer {
    background-image: none;
    padding-top: 10px;
}
.age{
    width: 70px; 
}
.status {
    vertical-align: middle;
}
.case {
    width: 15%;
    padding-left: 10px;
}
.updated {
    width: 15%;
    padding-left: 10px;
}
.formcontainer{
    float: left;
    text-align: left;
    width: 90px;
    color: #E3C51E;
}
#datatable {
    width: 100%;
    background-color: #000;
}
#datatable tr td {
    vertical-align: middle;
    padding: 1px;
}

#datatable table {
    background-image: url({!$Resource.item_row_bg});
    background-repeat: repeat-x;
    background-position: center center;
    border-width: 1px;
    border-style: solid;
    border-color: #52A3FF;
    height: 120px;
}
#datatable table tr {
    height: 0px;
}
#datatable table tr.divider, #datatable table tr.divider:hover {
    background: none;
    height: 25px;
}
#datatable table tr.divider td, #datatable table tr.divider:hover td {
    background-color: #000;
    height: 25px;
    border: none;
}
#datatable table tr td {
    vertical-align: top;
    height: 50%;
    padding-top: 5px;
    padding-right: 0px;
    padding-bottom: 0px;
    padding-left: 5px;
    color:#FFF;
    text-align: left;
}
#datatable table tr td.age {
    display: table-cell;
    vertical-align: middle;
    font-size: 130%;
    font-weight: bold;
    padding: 0px;
    padding-left: 20px;
    text-align: center;
}
#datatable table tr td.age.spex {
    background-image: url({!$Resource.spex});
    background-repeat: no-repeat;
    background-position: left center;
}
#datatable table tr td.age.tech {
    background-image: url({!$Resource.tech});
    background-position: 0 center;
    background-repeat: no-repeat;
}
#datatable table tr td.age .tag {
    background-image: url({!$Resource.bb_tag_sprite});
    background-repeat: no-repeat;
    height: 60px;
    width: 65px;
    padding-top: 7px;
    padding-right: 0px;
    padding-bottom: 7px;
    padding-left: 0px;    
}
#datatable table tr .age div div {
    font-size: 80%;
    font-family: Arial, Helvetica, sans-serif;
    font-weight: normal;
    padding-top: 10px;
}
#datatable table tr td.age .Red {
    background-position: -130px 0px;
}
#datatable table tr td.age .Red div {
    color: #C48888;
}
#datatable table tr td.age .Yellow {
    background-position: -65px 0px;
}
#datatable table tr td.age .Yellow div {
    color: #EADCAC;
}
#datatable table tr td.age .Green {
    background-position: 0px 0px;
}
#datatable table tr td.age .Green div {
    color: #99B995;
}
#datatable table tr td.status {
    font-size: 80%;
    font-weight: bold;
}
#datatable table tr td .star {
    background-image: url({!$Resource.bb_star_sprite});
    background-position: 0px 0px;
    background-repeat: no-repeat;
    height: 15px;
    width: 50px;
    margin-left: 7px;
    margin-top: 15px;
}
#datatable table tr td .star.one {
    background-position: 0px -25px;
    width: 35px;
}
#datatable table tr td .star.half {
    background-position: 0px -50px;
    width: 35px;
}
#datatable table tr td .star.blue {
    background-position: -50px 0px;
}
#datatable table tr td .star.green {
    background-position: -100px 0px;
}
#datatable table tr td .star.blue.one {
    width: 35px;
    background-position: -50px -25px;
}
#datatable table tr td .star.blue.half {
    width: 35px;
    background-position: -50px -50px;
}
#datatable table tr td .star.green.one {
    width: 35px;
    background-position: -100px -25px;
}
#datatable table tr td .star.green.half{
    width: 35px;
    background-position: -100px -50px;
}
#datatable table tr td .two {
    width: 35px;
    margin-right: 15px;
}
#datatable table tr td .three {
}
#datatable table tr td.updated {
    font-size: 160%;
    letter-spacing: 1em;
    font-weight: bolder;
}
#datatable table tr td.updated div {
    padding: 10px;
    margin-top: 10px;
    background-color: #333;
    border: 1px solid #FFF;
    margin-right: 10px;
}
#datatable table p {
    font-size: 80%;
    margin: 0px;
}
#datatable table p span {
    color: #73A7E0;
}
</style>
</head>
<body>
<table width="100%" border="0" align="center" cellpadding="0" cellspacing="0" id="datatable">
  <tr>
    <th valign="bottom">
    <a name="top"></a>
  <!-- <a href="{!serverUrl}{!IF(ontest,"RemovalsReport/","apex/")}Removals_mobile" class="button" style="float:right">Refresh</a> -->
     </th>
  </tr>
  <apex:repeat value="{!removals}" var="remov" >
  <tr>
    <td align="center">
    <!-- <apex:outputLink value="AOG_getCase_mobile?CASE={!cas.Id}" > -->
      <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
         <td rowspan="2" align="center" valign="middle" style="50px" class="age"> InService Impact
          <div class="tag {!remov.RPN__c}"> {!remov.In_Service_Impact__c} </div >
           </td>
          <td rowspan="2" align="left" valign="middle" style="50px" class="age"> AGE   <br>  </br>    
          <div class="tag {!remov.Removals_Service_Level__c}"> {!remov.Open_Age_in_Days__c}</div >
           </td>
          <td class="PNRemoved"> Removed P/N : 
           {!remov.Removed_P_N__c} <br />
          <span class="unitname"> Unit Name : {!remov.Unit_Name__c}</span><br/>
          <!-- <span class="Failuredate">{!remov.Failure_Date__c}</span><br /> -->
          <span>Failure Date : <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
        <apex:param value="{!remov.Failure_Date__c}" /> 
          </apex:outputText></span><br/>
          <span class="status"> Root Cause Status :{!remov.Root_Cause_Status__c}</span><br />
          <span class="carnumber">CAR Number : {!remov.CAR_Number__c}</span><br />
          <span class="resp">Project Engineer : {!remov.Project_Engineer__c}</span><br />
          <span class="resp1">Program Manager  : {!remov.Program_Manager__c}</span><br />
         </td>
        </tr>
    </table>
    </td>
  </tr>
  </apex:repeat>
  <th class="footer">
 <!-- <a href="https://qa-honeywellaero.cs14.force.com/Dashboards/Removals_Mobile" class="button">Refresh</a> -->
  </th>
</table>
      <div>
            <h3>A350 Legend</h3>
            <h4>What do these lists represent?</h4>
        </div>  
      <table border="1" align="left" cellpadding="10" cellspacing="10"> 
       <tr>
       <th>COLOUR</th>
       <th>IN-SERVICE IMPACT</th>
       <th>AGE</th>
       </tr>
       <tr>
      <td BGCOLOR="#4AA02C" style="width:200px">&nbsp;</td>
       <td style="width:600px">NA</td>
       <td style="width:600px">Age less than 20 days</td>
       </tr>
       <tr>
      <td BGCOLOR="#FDD017" style="width:200px">&nbsp;</td>
       <td style="width:600px">RPN between 0-45</td>
       <td  style="width:600px">Age between 20-30 days</td>
       </tr>
       <tr>
      <td BGCOLOR="#E41B17" style="width:200px">&nbsp;</td>
       <td style="width:600px">RPN greater than 46 </td>
       <td style="width:600px">Age greater than 30 days</td>
       </tr>
       </table>
</body>
</html>
<apex:insert name="footer">
        <c:honeywellFooter1 />
</apex:insert>

</apex:page>