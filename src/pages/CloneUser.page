<apex:page id="thePage" standardController="User" >
<apex:form >
<script>
function Clone(){
var url=[location.protocol+'//'+location.host]+'/{!User.Id}/e?clone=1&retURL=%2F{!User.Id}&name_firstName=&name_lastName=-&Alias=-&Email=-&Username=-&CommunityNickname=-';

window.parent.location=(url);

}
</script>
<center>
<input value="Clone User"  class="btn" name="attachFile" onclick="Clone();"  type="button" /> 
</center>

</apex:form>
</apex:page>