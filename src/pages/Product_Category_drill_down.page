<script type="text/javascript">
 
    function getProducttype() {
    //alert();
     var objName = document.getElementById("filter3").value;
     Visualforce.remoting.Manager.invokeAction(
     '{!$RemoteAction.DashboardHome.RemFeedback}',objName,
     function(result, event) {   
    if (event.status) {
    //alert(result.length);
     var l = result.length;
     if (l == 0) {
     document.getElementById("tab2").innerHTML = 'No Results!';
     } else {
     var markup = '<table id="records">';
     markup += '<tr ><th>FEE Number</th><th>Product category</th><th>Account Name</th><th>Year</th><th>SBU</th><th>Customer Rating</th></tr>';
     for (var i = 0; i< l; i++)
       {
     var recordDetail = '<tr>' + "<td>" + result[i].Name + "</td>" + "<td>" + result[i].Product_Category__c + "</td>" + "<td>" + 
     result[i].Account_Name__c + "</td>" + "<td>" + result[i].Year__c+ "</td>" + "<td>" + result[i].VOC_SBU__c + "</td>"+"<td>" + "<div id="+getcolor(result[i].Overall_Rating_Color__c)+">"+
                       "</div>" + "</td>" + '</tr>';
     markup += recordDetail;
     }
     document.getElementById("tab2").innerHTML = markup;
     }
     } else if (event.type === 'exception') {
     document.getElementById("tab2").innerHTML = event.message + "<br/>\n<pre>" + event.where + "</pre>";
     } else {
     document.getElementById("tab2").innerHTML = event.message;
     }
     }, 
     {
     escape: true
     });
    //alert('end');
    }
    
    function getcolor(cvalue){
   //alert(cvalue);
 if(cvalue=='Red')
 {
 return 'Red_Arrow7Overall';
 }
 if(cvalue=='Yellow')
 {
 return 'Red_Arrow11Overall';
 }
 if(cvalue=='Green')
 {
 return 'Red_Arrow9Overall';
 }
 else
 return 'Red_Arrow12Overall';
 }
