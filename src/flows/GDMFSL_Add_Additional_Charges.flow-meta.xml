        <locationX>521</locationX>
        <locationY>198</locationY>
        <defaultConnector>
            <targetReference>Exit</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Exit flow</defaultConnectorLabel>
        <rules>
            <name>Valid_WO_status_and_Region_is_not_null</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>currWO.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Dispatched</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currWO.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>In Progress</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currWO.GDMFSL_Region__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>AssignRegiontoAdditionalCharges</targetReference>
            </connector>
            <label>Valid WO status and Region is not null</label>
        </rules>
    </decisions>
