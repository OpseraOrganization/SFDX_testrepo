    <decisions>
        <name>Is_Ship_To_Account_Exist</name>
        <label>Is Ship-To Account Exist?</label>
        <locationX>168</locationX>
        <locationY>403</locationY>
        <defaultConnector>
            <targetReference>Ship_To_Not_Exist_Screen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not Exist</defaultConnectorLabel>
        <rules>
            <name>Exist</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>IsShiptoExist</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Select_contact_to_update</targetReference>
            </connector>
            <label>Exist</label>
        </rules>
    </decisions>
