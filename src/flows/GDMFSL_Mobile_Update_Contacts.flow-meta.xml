        </outputAssignments>
        <sortField>GDMFSL_Contact_Name__c</sortField>
        <sortOrder>Asc</sortOrder>
        <valueField>GDMFSL_Contact_Name__c</valueField>
    </dynamicChoiceSets>
    <formulas>
        <name>IsShiptoExist</name>
        <dataType>Boolean</dataType>
        <expression>IF (ISBLANK({!woRecord.GDMFSL_Ship_To_Account__c}),false,true)</expression>
    </formulas>
