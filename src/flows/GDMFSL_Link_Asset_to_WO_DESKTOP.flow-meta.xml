            <fieldType>DisplayText</fieldType>
        </fields>
        <showFooter>true</showFooter>
        <showHeader>false</showHeader>
    </screens>
    <screens>
        <name>screenLocationDoesNotMatch</name>
        <label>screenLocationDoesNotMatch</label>
        <locationX>535</locationX>
        <locationY>412</locationY>
        <allowBack>true</allowBack>
        <allowFinish>true</allowFinish>
        <allowPause>false</allowPause>
        <fields>
            <name>textLocationDoesNotMatch</name>
            <fieldText>&lt;p&gt;The location of the Asset does not match the location of the WO and cannot be added to the WO.&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <showFooter>true</showFooter>
        <showHeader>false</showHeader>
    </screens>
    <screens>
        <name>WOLI_Added</name>
        <label>WOLI Added</label>
        <locationX>1231</locationX>
        <locationY>298</locationY>
        <allowBack>false</allowBack>
        <allowFinish>true</allowFinish>
        <allowPause>false</allowPause>
        <fields>
            <name>WOLI_Created</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;background-color: rgb(255, 255, 255); color: rgb(62, 62, 60);&quot;&gt;The Asset with Digital Id ({!varAssetDigitalId}) has been associated with a Work Order Line Item on this Work Order.  Click Finish to return.&lt;/span&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <showFooter>true</showFooter>
        <showHeader>false</showHeader>
    </screens>
    <start>
        <locationX>50</locationX>
        <locationY>50</locationY>
        <connector>
            <targetReference>IsAssetNewAsset</targetReference>
        </connector>
    </start>
    <status>Active</status>
    <variables>
