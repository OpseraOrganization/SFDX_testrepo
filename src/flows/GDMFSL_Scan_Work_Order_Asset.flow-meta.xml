    <decisions>
        <description>Checks if an asset was selected</description>
        <name>AssetWasFoundAndSelected</name>
        <label>AssetWasFoundAndSelected</label>
        <locationX>777</locationX>
        <locationY>344</locationY>
        <defaultConnector>
            <targetReference>Asset_Not_Found_Msg</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Asset was selected or Found</defaultConnectorLabel>
        <rules>
            <name>Asset_was_selected</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>pklstAsset_Search_Result</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>NoMatchingAssetFound</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>pklstAsset_Search_Result</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Link_Asset_to_WO_if_it_is_not_yet_linked</targetReference>
            </connector>
            <label>Asset was selected</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Break_Fix</name>
        <label>Is Break Fix?</label>
        <locationX>448</locationX>
        <locationY>550</locationY>
        <defaultConnector>
            <targetReference>Not_Break_Fix_WO</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_Break_Fix</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varWorkTypeBreakFixId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>BF</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Search_Asset_Enter_Search_Values</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
