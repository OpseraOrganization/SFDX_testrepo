    <description>Report of NM&amp;SON cases where any of the 3 SLA milestones got missed/violated</description>
    <filter>
        <booleanFilter>1 AND 2 AND 3</booleanFilter>
        <criteriaItems>
            <column>CM.TARGET_DATE</column>
