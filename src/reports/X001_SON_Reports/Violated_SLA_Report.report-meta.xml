            <column>Case.CH_Product_Name__c</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>contains</operator>
            <value>NetAct,Eden,cSON,I-SDK,Archive Cloud,Network Element Backup,Network 360,DCAP,UTF</value>
        </criteriaItems>
        <language>en_US</language>
    </filter>
