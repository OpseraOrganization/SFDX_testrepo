<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>D_S_Account_Sharing_rule</fullName>
        <accessLevel>Read</accessLevel>
        <accountSettings>
            <caseAccessLevel>None</caseAccessLevel>
            <contactAccessLevel>None</contactAccessLevel>
            <opportunityAccessLevel>None</opportunityAccessLevel>
        </accountSettings>
        <label>D&amp;S Account Sharing rule</label>
        <sharedTo>
            <group>D_S_Partner_Portal_Group</group>
        </sharedTo>
        <criteriaItems>
            <field>Strategic_Business_Unit__c</field>
            <operation>equals</operation>
            <value>D&amp;S</value>
        </criteriaItems>
        <criteriaItems>
            <field>DSPC_Account__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingGuestRules>
        <fullName>W2C_Site_Guest_User_rule</fullName>
        <accessLevel>Read</accessLevel>
        <label>W2C Site Guest User rule</label>
        <sharedTo>
            <guestUser>W2C</guestUser>
        </sharedTo>
        <criteriaItems>
            <field>Name</field>
            <operation>notEqual</operation>
            <value>null</value>
        </criteriaItems>
        <includeHVUOwnedRecords>false</includeHVUOwnedRecords>
    </sharingGuestRules>
