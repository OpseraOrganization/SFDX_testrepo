    <rules>
        <fullName>Email Reminder</fullName>
        <actions>
            <name>Email_Reminder_2</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Reminder_Field_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>WorkFlow_Approval_History__c.Approver__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>WorkFlow_Approval_History__c.Reminder__c</field>
            <operation>equals</operation>
            <value>Scheduler</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>WF_Approved_Notification</fullName>
        <actions>
            <name>WF_Approved</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>WorkFlow_Approval_History__c.Approval_Status__c</field>
            <operation>equals</operation>
            <value>Approved</value>
        </criteriaItems>
        <criteriaItems>
            <field>WorkFlow_Approval_History__c.Old_record__c</field>
            <operation>notEqual</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>WorkFlow_Approval_History__c.LastModifiedById</field>
            <operation>notEqual</operation>
            <value>SFDC Admin</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
