    <rules>
        <fullName>Contract 2nd Anniversary Email - 3 years</fullName>
        <actions>
            <name>Contract_Renewal_Pre_expiration_year_notification</name>
            <type>Task</type>
        </actions>
        <active>true</active>
        <description>Workflow related to Contract Expiry Pre Notification (3yr contract)</description>
        <formula>AND(OR($RecordType.Name=&apos;HAPP/MPP Contracts&apos;, $RecordType.Name =&apos;MSP Contracts&apos;, $RecordType.Name =&apos;Read-Only HAPP/MPP Contracts&apos;, $RecordType.Name =&apos;Read-Only MSP Contracts&apos;) , ISPICKVAL(Term_of_Contract__c ,&apos;3 Years - Prepaid&apos;), Number_of_days_from_Contract_Start_Date__c =700,OR(ISPICKVAL( Status,&apos;Hold&apos;), ISPICKVAL( Status,&apos;Open&apos; )), OR(ISCHANGED( Number_of_days_from_Contract_Start_Date__c ),ISCHANGED(Status),ISCHANGED(Term_of_Contract__c)))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
