    <rules>
        <fullName>Contract Active Status Update based on site status</fullName>
        <actions>
            <name>Status_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>and( 
 LEFT( Site_Status__c , 7 ) == &apos;SAP_CNT&apos; ,
 !CONTAINS( LOWER( Site_Status__c ) , &apos;cls&apos; )
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
