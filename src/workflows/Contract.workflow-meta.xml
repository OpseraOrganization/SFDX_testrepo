    <rules>
        <fullName>Contract Expiration reminder before 3 and 6 months</fullName>
        <active>true</active>
        <booleanFilter>1 AND 2 AND 3 AND (4 and  5)</booleanFilter>
        <criteriaItems>
            <field>Contract.Contract_End_Date_Custom__c</field>
            <operation>notEqual</operation>
