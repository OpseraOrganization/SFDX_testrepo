        </criteriaItems>
        <criteriaItems>
            <field>Contract.Status</field>
            <operation>equals</operation>
            <value>Active</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contract.Type__c</field>
            <operation>notEqual</operation>
            <value>MSP</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contract.Type__c</field>
            <operation>notEqual</operation>
