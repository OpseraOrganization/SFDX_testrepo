    <rules>
        <fullName>SIP UAT Failed</fullName>
        <actions>
            <name>SIP_UAT_Failed</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>SIP_Tickets__c.Team_Ticket_Type__c</field>
            <operation>equals</operation>
            <value>UAT</value>
        </criteriaItems>
        <criteriaItems>
            <field>SIP_Tickets__c.UAT_Expected_v_Actual__c</field>
            <operation>contains</operation>
            <value>Fail</value>
        </criteriaItems>
        <criteriaItems>
            <field>SIP_Tickets__c.UAT_Status__c</field>
            <operation>notEqual</operation>
            <value>Not Started</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>SIP reminder submit</fullName>
        <active>false</active>
        <criteriaItems>
            <field>SIP_Tickets__c.Ticket_Pipeline__c</field>
            <operation>equals</operation>
            <value>1). Created,Not Submitted</value>
        </criteriaItems>
        <description>Reminder to submit created but not submitted ticket</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Please_submit_for_approval89</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>SIP_ticket_SIP_Tickets_c_Name_needs_to_be_submmited89</name>
                <type>Task</type>
            </actions>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
