    <rules>
        <fullName>Closed Ticket Updates</fullName>
        <actions>
            <name>SIP_Closed_Ticket_Notification</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Closed_Date_Populate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>SIP_Tickets__c.Ticket_Status__c</field>
            <operation>equals</operation>
            <value>Closed - Request Completed,Closed - Not all Requests Completed</value>
        </criteriaItems>
        <criteriaItems>
            <field>SIP_Tickets__c.UAT_Status__c</field>
            <operation>equals</operation>
            <value>Complete</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Comments to Comment History</fullName>
