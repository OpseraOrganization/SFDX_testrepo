        </actions>
        <actions>
            <name>Current_Comments_to_History</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>SIP_Tickets__c.Current_Comments__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Review and Assigned Notification</fullName>
        <actions>
            <name>Review_and_Assigned_Notification</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>SIP_Tickets__c.Ticket_Status__c</field>
            <operation>equals</operation>
            <value>Reviewed and Assigned</value>
        </criteriaItems>
        <criteriaItems>
            <field>SIP_Tickets__c.Reason_for_Request__c</field>
            <operation>notEqual</operation>
            <value>Requesting Xactly Access,NRComp,SIP Team Admin Ticket</value>
        </criteriaItems>
        <criteriaItems>
            <field>SIP_Tickets__c.Current_Comments__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
