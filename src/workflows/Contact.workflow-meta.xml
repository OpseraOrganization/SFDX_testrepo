    <rules>
        <fullName>Activate Sales Surveyed Field For German Contacts</fullName>
        <actions>
            <name>Sales_Surveyed_Should_be_Checked</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Sales Surveyed Should be Checked for German Users</description>
        <formula>LOWER(MailingCountry) =&apos;germany&apos; || LOWER(MailingCountry) =&apos;de&apos; || LOWER(MailingCountry) =&apos;deutschland&apos; ||  BEGINS (Phone, &quot;+49&quot;) ||  (CONTAINS(SUBSTITUTE(Email, LEFT(Email, FIND(&quot;@&quot;, Email)), NULL),&apos;.de&apos;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
