    <rules>
        <fullName>Contact Manager Change</fullName>
        <actions>
            <name>Contact_Manager_Change</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <formula>and(
 Contact_Is_Employee__c = true,
 ISCHANGED( User_Primary_Manager_EID__c ),
 not ISNULL( User__c ), 
 not ISBLANK( User__c ), 
or( 
 text( Employee_Contact_SBU__c ) = &quot;D&amp;S&quot;,
 PRIORVALUE(Employee_Contact_SBU__c) = &quot;D&amp;S&quot;,
 Left ( User__r.Profile.Name , 3 ) = &quot;D&amp;S&quot; 
)
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>EditProfileChange</fullName>
        <actions>
            <name>EditProfileChange_QA</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <description>Sending obm message when change in superUser role</description>
        <formula>AND(OR(ISCHANGED(Postal_Code__c),ISCHANGED(Contact_Function__c),ISCHANGED(Organizational_Level__c),ISCHANGED(Phone_1__c),
ISCHANGED(Primary_Email_Address__c), ISCHANGED(Address_Line_1__c),ISCHANGED(Address_Line_2__c),ISCHANGED(Address_Line_3__c),
ISCHANGED(Country_Name__c),ISCHANGED(State_Code__c),ISCHANGED(MobilePhone),ISCHANGED(City_Name__c),ISCHANGED(LastName),
ISCHANGED(FirstName),ISCHANGED(Country_Code__c),ISCHANGED(Account_Name__c),ISCHANGED(Contact_Birth_Country__c),ISCHANGED(Birth_City__c),ISCHANGED(Is_US_Citizen__c),ISCHANGED(Citizenship_Country__c),
ISCHANGED(Job_Title__c),ISCHANGED(Permanent_USA_Resident__c),ISCHANGED(Permanent_Resident_Expiration_Date__c)),NOT(ISBLANK(Honeywell_ID__c)),NOT(ISBLANK(Postal_Code__c)),NOT(ISBLANK(TEXT(Country_Name__c))),
NOT(ISBLANK(City_Name__c)),NOT(ISBLANK(Address_Line_1__c)),NOT(ISPICKVAL(Contact_Status__c, &quot;InActive&quot;))  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
