    <rules>
        <fullName>Strap Account Change</fullName>
        <actions>
            <name>Strap_Account_Change</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>and(
ISCHANGED(  Account__c  ),
not ISBLANK(  Account__c  ),
not ISNULL(  Account__c  ) 
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
