    <rules>
        <fullName>CIJ Owner Update for Audit Type</fullName>
        <actions>
            <name>CIJ_Audit_Owner_update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>CIJ owner update for Audit record type</description>
        <formula>RecordType.Name = &apos;Audit CIJ&apos; &amp;&amp; (NOT(CONTAINS(Customer__r.Name, &quot;BOEING&quot;)) &amp;&amp; NOT(CONTAINS(Customer__r.Name, &quot;LOCKHEED&quot;)))</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>CIJ Owner Update for CBT- DAA%2FDSE</fullName>
        <actions>
            <name>CIJ_CBTUpdate5</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>(CBT__c = &apos;DAA&apos; || CBT__c = &apos;DSE&apos; || RecordType.Name = &apos;Transactional CIJ&apos;) &amp;&amp; (RecordType.Name &lt;&gt; &apos;Audit CIJ&apos; &amp;&amp; NOT(CONTAINS(Customer__r.Name, &quot;BOEING&quot;)) &amp;&amp;  NOT(CONTAINS(Customer__r.Name, &quot;LOCKHEED&quot;)))</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
