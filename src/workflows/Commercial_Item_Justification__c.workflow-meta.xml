    <rules>
        <fullName>CIJ Owner Update for CBT- DMA</fullName>
        <actions>
            <name>CIJ_DMA_Owner_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>(ISNULL(CBT__c) || CBT__c = &apos;DMA&apos; || CBT__c = &apos;MS&amp;C OEM&apos; || CONTAINS(Customer__r.Name, &quot;LOCKHEED&quot;) || (CBT__c &lt;&gt; &apos;DAP&apos; &amp;&amp; CBT__c &lt;&gt; &apos;DMM&apos; &amp;&amp; CBT__c &lt;&gt; &apos;DSPC&apos; &amp;&amp; CBT__c &lt;&gt; &apos;DBL&apos; &amp;&amp; CBT__c &lt;&gt; &apos;DAA&apos; &amp;&amp; CBT__c &lt;&gt; &apos;DSE&apos;)) &amp;&amp; (RecordType.Name &lt;&gt; &apos;Audit CIJ&apos; &amp;&amp; RecordType.Name &lt;&gt; &apos;Transactional CIJ&apos; &amp;&amp; NOT(CONTAINS(Customer__r.Name, &quot;BOEING&quot;)))</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
