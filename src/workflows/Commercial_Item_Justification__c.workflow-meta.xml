        </actions>
        <active>true</active>
        <formula>(CBT__c = &apos;DAP&apos; || CBT__c = &apos;DSPC&apos;) &amp;&amp; (RecordType.Name &lt;&gt; &apos;Audit CIJ&apos; &amp;&amp; RecordType.Name &lt;&gt; &apos;Transactional CIJ&apos; &amp;&amp; NOT(CONTAINS(Customer__r.Name, &quot;BOEING&quot;)) &amp;&amp; NOT(CONTAINS(Customer__r.Name, &quot;LOCKHEED&quot;)))</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
