    <rules>
        <fullName>CIJ Owner upadate for CBT is Null</fullName>
        <actions>
            <name>update_CIJ_owner</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <booleanFilter>(1 AND 2) OR ( 2 AND 3 AND 4 AND 5 AND 6 AND 7 AND 8 AND 9 AND 10)</booleanFilter>
        <criteriaItems>
            <field>Commercial_Item_Justification__c.CBT__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Commercial_Item_Justification__c.RecordTypeId</field>
            <operation>notEqual</operation>
            <value>Audit CIJ</value>
        </criteriaItems>
        <criteriaItems>
            <field>Commercial_Item_Justification__c.CBT__c</field>
            <operation>notEqual</operation>
            <value>DAA</value>
        </criteriaItems>
        <criteriaItems>
            <field>Commercial_Item_Justification__c.CBT__c</field>
            <operation>notEqual</operation>
            <value>DSE</value>
        </criteriaItems>
        <criteriaItems>
            <field>Commercial_Item_Justification__c.CBT__c</field>
            <operation>notEqual</operation>
            <value>DAP</value>
        </criteriaItems>
        <criteriaItems>
            <field>Commercial_Item_Justification__c.CBT__c</field>
            <operation>notEqual</operation>
            <value>DSPC</value>
        </criteriaItems>
        <criteriaItems>
            <field>Commercial_Item_Justification__c.CBT__c</field>
            <operation>notEqual</operation>
            <value>DMM</value>
        </criteriaItems>
        <criteriaItems>
            <field>Commercial_Item_Justification__c.CBT__c</field>
            <operation>notEqual</operation>
            <value>DBL</value>
        </criteriaItems>
        <criteriaItems>
            <field>Commercial_Item_Justification__c.CBT__c</field>
            <operation>notEqual</operation>
            <value>MS&amp;C OEM</value>
        </criteriaItems>
        <criteriaItems>
            <field>Commercial_Item_Justification__c.CBT__c</field>
            <operation>notEqual</operation>
            <value>DMA</value>
        </criteriaItems>
        <description>update the owner when CBT field is NULL and not belong to existing CBT logic</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
