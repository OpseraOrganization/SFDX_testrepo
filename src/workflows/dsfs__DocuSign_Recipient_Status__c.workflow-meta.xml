<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <fieldUpdates>
        <fullName>DocuSign_Status_Envelope_Status</fullName>
        <field>dsfs__Envelope_Status__c</field>
        <formula>IF( dsfs__Recipient_Status__c = &quot;Sent&quot; , &quot;Contract Returned-HON&quot;,
&quot;&quot;

)</formula>
        <name>DocuSign Status Envelope Status</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
        <targetObject>dsfs__Parent_Status_Record__c</targetObject>
    </fieldUpdates>
    <rules>
        <fullName>Change Sent status on DocuSign Status Object</fullName>
        <actions>
            <name>DocuSign_Status_Envelope_Status</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND 2 AND 3 AND 4</booleanFilter>
        <criteriaItems>
            <field>dsfs__DocuSign_Recipient_Status__c.Name</field>
            <operation>equals</operation>
            <value>MSP Contracts</value>
        </criteriaItems>
        <criteriaItems>
            <field>dsfs__DocuSign_Recipient_Status__c.dsfs__DocuSign_Recipient_Email__c</field>
            <operation>equals</operation>
            <value>mspcontracts@honeywell.com</value>
        </criteriaItems>
        <criteriaItems>
            <field>dsfs__DocuSign_Recipient_Status__c.dsfs__DocuSign_Routing_Order__c</field>
            <operation>equals</operation>
            <value>3</value>
        </criteriaItems>
        <criteriaItems>
            <field>dsfs__DocuSign_Recipient_Status__c.dsfs__Recipient_Status__c</field>
            <operation>equals</operation>
            <value>Sent</value>
        </criteriaItems>
        <description>Envelope status on DocuSign Object will be changed</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
