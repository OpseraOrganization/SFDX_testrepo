    <rules>
        <fullName>AOG Case TSLI Update</fullName>
        <actions>
            <name>AOG_Case_TSLI_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Updates the TSLI date field based on latest outbound email sent to the customer</description>
        <formula>AND(OR(Contains(ToAddress, Parent.Contact.Primary_Email_Address__c),Contains(ToAddress, Parent.SuppliedEmail)) ,or(Parent.RecordType.Id = &apos;01230000000Zemy&apos;,      Parent.RecordType.Id = &apos;0122g0000001jVwA &apos;),   or (ISPICKVAL(Status, &quot;Sent&quot;), ISPICKVAL(Status, &quot;Replied&quot;)))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
