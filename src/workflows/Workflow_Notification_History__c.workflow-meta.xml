        <name>WNHRejectedFieldUpdate</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <rules>
        <fullName>Workflow Approved Notification</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Workflow_Notification_History__c.WF_Status__c</field>
            <operation>equals</operation>
            <value>Approved</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Workflow Rejected Notification to Notifiers</fullName>
        <actions>
            <name>Wf_reject_2</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>WNHRejectedFieldUpdate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Workflow_Notification_History__c.WNHRejected__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Workflow_Notification_History__c.LastModifiedById</field>
            <operation>notEqual</operation>
            <value>SFDC Admin</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
</Workflow>
