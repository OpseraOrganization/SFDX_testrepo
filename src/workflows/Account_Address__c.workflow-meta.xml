    <rules>
        <fullName>Account Address SBL Account Address Row ID</fullName>
        <actions>
            <name>Account_Address_SBL_Account_Address_Row</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Account Address SBL Account Address Row ID</description>
        <formula>and(
or(
 LEFT( Sbl_Account_Address_Row_ID__c , 2 ) = &quot;FA&quot;,
 LEFT( Sbl_Account_Address_Row_ID__c , 3 ) = &quot;BMS&quot;, 
 LEFT( Sbl_Account_Address_Row_ID__c , 4 ) = &quot;SIMS&quot;
),
or(
 ISBLANK( Site_Id__c ),
 ISNULL( Site_Id__c )
),
or(
 ISBLANK( Site_Name__c ),
 ISNULL( Site_Name__c ) 
)
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Account Address-Notification mail to Account Owner when Denied Party Status is Blocked</fullName>
        <actions>
            <name>Notification_when_Denied_Party_status_blocked</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>ISCHANGED(Denied_Party_Status__c)&amp;&amp; ISPICKVAL(Denied_Party_Status__c,&apos;Blocked&apos;)
&amp;&amp; ISPICKVAL(PRIORVALUE(Denied_Party_Status__c),&apos;Further Review Needed&apos;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Account Address-Notification mail when Denied party status Released</fullName>
        <actions>
