        </actions>
        <active>true</active>
        <formula>ISCHANGED(Denied_Party_Status__c)&amp;&amp;(ISPICKVAL(Denied_Party_Status__c,&apos;Released&apos;) )
&amp;&amp; ISPICKVAL(PRIORVALUE(Denied_Party_Status__c),&apos;Blocked&apos;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
