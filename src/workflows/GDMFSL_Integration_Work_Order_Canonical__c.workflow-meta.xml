        <apiVersion>50.0</apiVersion>
        <endpointUrl>https://it.api.honeywell.com/hbt/fsl/service-order/v1/close/sfdctosap?api-key=zxDN9ZeEPFw6TVug7IUCAleA9HiWuaaG</endpointUrl>
        <fields>Chargeable__c</fields>
        <fields>Id</fields>
        <fields>Invoice_Text__c</fields>
        <fields>Job_Type__c</fields>
        <fields>Name</fields>
        <fields>PO_Number__c</fields>
        <fields>Related_Work_Order__c</fields>
        <fields>Service_Order_Number__c</fields>
        <fields>Status__c</fields>
        <fields>Work_Order_Number__c</fields>
        <includeSessionId>true</includeSessionId>
        <integrationUser>interface.user1@honeywell.com.hbtprod</integrationUser>
        <name>Create SO TECO Outbound to APIGEE</name>
        <protected>false</protected>
        <useDeadLetterQueue>false</useDeadLetterQueue>
