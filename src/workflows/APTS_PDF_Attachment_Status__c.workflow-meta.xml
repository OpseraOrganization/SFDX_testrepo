<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <outboundMessages>
        <fullName>APTS_Send_CreditMemo_Attachment</fullName>
        <apiVersion>42.0</apiVersion>
        <endpointUrl>https://it.api.honeywell.com/sfdc/aero/apttus/creditmemo/billingdocument</endpointUrl>
        <fields>Id</fields>
        <includeSessionId>false</includeSessionId>
        <integrationUser>karanatakam.ganguraju@honeywell.com.aero</integrationUser>
        <name>APTS Send CreditMemo Attachment</name>
        <protected>false</protected>
        <useDeadLetterQueue>false</useDeadLetterQueue>
    </outboundMessages>
    <outboundMessages>
        <fullName>APTS_Send_Invoice_Attachment</fullName>
        <apiVersion>42.0</apiVersion>
        <endpointUrl>https://it.api.honeywell.com/sfdc/aero/apttus/invoice/billingdocument</endpointUrl>
        <fields>Id</fields>
        <includeSessionId>false</includeSessionId>
        <integrationUser>karanatakam.ganguraju@honeywell.com.aero</integrationUser>
        <name>APTS Send Invoice Attachment</name>
        <protected>false</protected>
        <useDeadLetterQueue>false</useDeadLetterQueue>
    </outboundMessages>
    <rules>
        <fullName>APTS Attachment Integration Status CreditMemo</fullName>
        <actions>
            <name>APTS_Send_CreditMemo_Attachment</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <description>APTS Attachment Integration Status CreditMemo</description>
        <formula>NOT(ISNULL( APTS_Credit_Memo__c ) &amp;&amp; NOT(ISBLANK(APTS_Credit_Memo__c)) ) &amp;&amp; ISPICKVAL(APTS_Status__c, &quot;To be Processed&quot;)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>APTS Attachment Integration Status Invoice</fullName>
        <actions>
            <name>APTS_Send_Invoice_Attachment</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <description>APTS Attachment Integration Status Invoice</description>
        <formula>NOT(ISNULL(APTS_Invoice__c) &amp;&amp; NOT(ISBLANK(APTS_Invoice__c)) ) &amp;&amp; ISPICKVAL(APTS_Status__c, &quot;To be Processed&quot;)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
</Workflow>
