<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>APTS_Submit__c</fullName>
    <description>Formula field to show the Submite button/hyperlink.  Formula utilizes Status to determine whether hyperlink/button is displayed.</description>
    <externalId>false</externalId>
    <formula>IF ((ISPICKVAL(Apttus_Billing__Status__c, &quot;Draft&quot;)&amp;&amp; ISPICKVAL(APTS_Integration_Status__c, &quot;Completed&quot;)), 
HYPERLINK(&quot;/apex/APTS_InvoiceSubmit?id=&quot;&amp;Id, IMAGE(&quot;/resource/APTS_Button_Submit&quot;, &quot;Submit&quot;),&quot;_self&quot;), NULL)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Submit Invoice</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
