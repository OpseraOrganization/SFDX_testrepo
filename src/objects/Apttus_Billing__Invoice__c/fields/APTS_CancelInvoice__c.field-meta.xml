<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>APTS_CancelInvoice__c</fullName>
    <description>Formula field to show the Cancel Invoice button/hyperlink.  Formula utilizes Status to determine whether hyperlink/button is displayed.</description>
    <externalId>false</externalId>
    <formula>IF (ISPICKVAL(Apttus_Billing__Status__c, &quot;Draft&quot;), 
HYPERLINK(&quot;/apex/APTS_CancelInvoice?id=&quot;&amp;Id, IMAGE(&quot;/resource/APTS_CancelInvoice&quot;, &quot;Submit&quot;),&quot;_self&quot;), NULL)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Cancel Invoice</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
