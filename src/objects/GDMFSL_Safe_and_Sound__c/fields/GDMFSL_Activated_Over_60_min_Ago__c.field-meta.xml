<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GDMFSL_Activated_Over_60_min_Ago__c</fullName>
    <description>Field to alert Data Specialists that Safe and Sound was activated more than 60 minutes ago, per DTXFSL-6899</description>
    <externalId>false</externalId>
    <formula>(ISPICKVAL(  GDMFSL_Status__c , &apos;Activated&apos;)  &amp;&amp; 
( NOW()- CreatedDate &gt; 0.0416))</formula>
    <label>Activated Over 60 min Ago</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
