<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_check_for_Expected_LOA_Level</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
Opportunity__r.OfferWithoutExpectedLoALevel__c&gt;0,
$Profile.Name&lt;&gt; $Label.Data_Loader_Profile_Name 
)</errorConditionFormula>
    <errorMessage>Please populate Expected LoA Level against all existing Offers for this Opportunity.</errorMessage>
</ValidationRule>
