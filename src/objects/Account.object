    <fields>
        <fullName>SalesLoft1__Active_Account__c</fullName>
        <deprecated>false</deprecated>
        <description>If Account has an activity logged within the last 30 days, this field will return a value of 1.</description>
        <externalId>false</externalId>
        <formula>IF( TODAY () - LastActivityDate &lt;= 30, 1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Active Account</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SalesLoft1__SalesLoft_Domain__c</fullName>
        <deprecated>false</deprecated>
        <description>This field is used for syncing Account records with SalesLoft. A &apos;domain&apos; value is a required in a URL format in order for Account records to sync with SalesLoft.</description>
        <externalId>false</externalId>
        <formula>IF((CONTAINS(Website,&quot;.&quot;)&amp;&amp;(NOT(CONTAINS(Website,&quot; &quot;))))
,Website+&apos;?&apos;+CASESAFEID(Id),CASESAFEID(Id)+&apos;.com&apos;)</formula>
        <inlineHelpText>This field is used for syncing Account records with SalesLoft.</inlineHelpText>
        <label>SalesLoft Domain</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
