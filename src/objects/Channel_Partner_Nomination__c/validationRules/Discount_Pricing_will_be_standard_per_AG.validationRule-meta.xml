<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Discount_Pricing_will_be_standard_per_AG</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ispickval(Discount_Pricing_will_be_standard_per_A__c, &quot;No&quot;), ISBLANK(If_No_Explain_Non_Standard__c)
)</errorConditionFormula>
    <errorDisplayField>ACC_Approver__c</errorDisplayField>
    <errorMessage>Since &quot;No&quot; has been selected for the field Discount/Pricing Standard per AG-5801, please provide an explanation in the field Non-Standard Terms.</errorMessage>
</ValidationRule>
