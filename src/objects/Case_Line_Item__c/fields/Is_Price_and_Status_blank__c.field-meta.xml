<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Price_and_Status_blank__c</fullName>
    <description>Return 1 if both Price and Price Status are blank</description>
    <externalId>false</externalId>
    <formula>IF(
   AND(	
    ISBLANK(TEXT(Price_Status__c)),
    ISNULL(Price__c)
   ),
1,0)</formula>
    <inlineHelpText>System Calculation: Returns 1 if both Price and Price Status are blank</inlineHelpText>
    <label>Is Price and Status blank</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
