<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Target_Completion_Must_Be_in_Future</fullName>
    <active>true</active>
    <errorConditionFormula>Target_Completion_Date__c &lt; Today()  &amp;&amp; ISNEW()</errorConditionFormula>
    <errorDisplayField>Target_Completion_Date__c</errorDisplayField>
    <errorMessage>Target Completion Date must be in the future.</errorMessage>
</ValidationRule>
