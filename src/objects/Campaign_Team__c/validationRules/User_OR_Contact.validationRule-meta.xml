<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>User_OR_Contact</fullName>
    <active>true</active>
    <errorConditionFormula>OR(and(Team_User__c!=null,Contact__c!=null),and(Team_User__c==null,Contact__c==null))</errorConditionFormula>
    <errorMessage>Please select either user or a contact for the campaign team</errorMessage>
</ValidationRule>
