            <value>VARIOUS</value>
        </filterItems>
        <infoMessage>Account Status should not be inactive and Account Name should not contain Various</infoMessage>
        <isOptional>false</isOptional>
    </lookupFilter>
    <trackHistory>false</trackHistory>
    <type>Lookup</type>
</CustomField>
