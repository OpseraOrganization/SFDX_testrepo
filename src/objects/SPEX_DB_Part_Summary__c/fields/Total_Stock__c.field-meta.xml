<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Stock__c</fullName>
    <description>Total Stock is the sum of the stock values for regions Americas, APAC and EMEA</description>
    <externalId>false</externalId>
    <formula>Stock_By_Americas__c  +  Stock_By_APAC__c  +  Stock_By_EMEA__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Stock</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
