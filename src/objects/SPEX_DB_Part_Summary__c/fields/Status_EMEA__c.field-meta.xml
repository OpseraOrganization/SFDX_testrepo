<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status_EMEA__c</fullName>
    <description>If Stockout value for EMEA region is greater than 0, then status is Stock_Out. If  Stockout is 0 , then status is At_Risk. Else the status is safe.</description>
    <externalId>false</externalId>
    <formula>IF( Stockout_EMEA__c &gt; 0, &apos;Stock_Out&apos;, IF(Min_Stock_EMEA__c = 0, &apos;Grey&apos;, IF( ((Stockout_EMEA__c = 0)), &apos;At_Risk&apos;, &apos;Safe&apos;)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Status EMEA</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
