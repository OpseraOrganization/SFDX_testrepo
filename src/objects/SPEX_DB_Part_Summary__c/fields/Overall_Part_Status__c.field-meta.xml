<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Overall_Part_Status__c</fullName>
    <description>If Stockout value for is greater than 0, then status is Stock_Out. If  Stockout value is 0 , then status is At_Risk. Else the status is safe.</description>
    <externalId>false</externalId>
    <formula>IF(  Stockout__c &gt; 0, &apos;Stock_Out&apos;, IF(Stockout__c  = 0, &apos;At_Risk&apos;, &apos;Safe&apos;)  )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Overall_Part_Status</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
