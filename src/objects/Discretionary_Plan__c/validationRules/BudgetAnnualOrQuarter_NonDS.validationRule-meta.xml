<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BudgetAnnualOrQuarter_NonDS</fullName>
    <active>true</active>
    <errorConditionFormula>and( 
 Approved_Budget__c &lt;&gt; 0, 
