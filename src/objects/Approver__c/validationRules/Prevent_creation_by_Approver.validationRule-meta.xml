<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_creation_by_Approver</fullName>
    <active>true</active>
    <errorConditionFormula>AND(  ISNEW(), CONTAINS($Profile.Name, &quot;Approver&quot;), NOT(CONTAINS($Profile.Name, &quot;Admin&quot;)))</errorConditionFormula>
    <errorMessage>Approvers cannot create new records, please contact Admin approvers</errorMessage>
</ValidationRule>
