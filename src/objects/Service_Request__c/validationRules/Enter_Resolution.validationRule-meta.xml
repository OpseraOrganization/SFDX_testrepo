<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Enter_Resolution</fullName>
    <active>false</active>
    <description>Enter Resolution</description>
    <errorConditionFormula>and(      ispickval( Status__c ,&quot;Closed&quot;) ,Status_Resolution__c ==null)</errorConditionFormula>
    <errorDisplayField>Status_Resolution__c</errorDisplayField>
    <errorMessage>Please Enter Resolution Before Closing</errorMessage>
</ValidationRule>
