<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Customer_Initiated_Date_Required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL( Issue_Type__c , &apos;Non-Technical&apos;) , ISNULL( Customer_Initiated_Date__c ) )</errorConditionFormula>
    <errorDisplayField>Customer_Initiated_Date__c</errorDisplayField>
    <errorMessage>Filed &apos;Customer Initiated Date&apos; is mandatory when issue type is selected as &apos;Non-Technical&apos;.</errorMessage>
</ValidationRule>
