<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Last_Updated_Difference__c</fullName>
    <description>If Today - Last Modified Date is less than Expected Update Period then True (1), otherwise False (0)</description>
    <externalId>false</externalId>
    <formula>If( Expected_Update_Period__c=0,If(Today()-DateValue(LastModifiedDate)&lt;=14,1,0),If(Today()-DateValue(LastModifiedDate)&lt;=Expected_Update_Period__c,1,0))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Last Updated Difference</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
