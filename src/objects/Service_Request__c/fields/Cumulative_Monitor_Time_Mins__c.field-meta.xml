<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cumulative_Monitor_Time_Mins__c</fullName>
    <externalId>false</externalId>
    <formula>IF((ISPICKVAL(Status__c , &apos;Monitor&apos;)), 
IF(ISNULL(total_days_in_monitor__c ), 
ROUND(MOD((now() - open_to_monitor__c)*1440,60),0) 
,  
ROUND((MOD((now() - open_to_monitor__c)*1440,60)),0)+total_days_in_monitor__c) 
,(total_days_in_monitor__c))</formula>
    <label>Cumulative Monitor Time(Mins)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
