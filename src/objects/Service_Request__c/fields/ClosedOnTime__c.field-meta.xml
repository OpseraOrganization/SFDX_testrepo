<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ClosedOnTime__c</fullName>
    <description>If Date Closed is Less than or Equal to Customer Due Date + 24 hours then True otherwise False</description>
    <externalId>false</externalId>
    <formula>if( Date_Closed__c &lt;=  Customer_Due_Date__c+1 ,1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>ClosedOnTimeCustDueDate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
