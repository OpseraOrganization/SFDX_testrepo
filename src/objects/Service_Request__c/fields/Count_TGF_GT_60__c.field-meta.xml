<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Count_TGF_GT_60__c</fullName>
    <description># Count TGF &gt; 60 (1 : timebetween(Closed Date,Created Date) &gt;= 60 Days = True :: 0 otherwise)</description>
    <externalId>false</externalId>
    <formula>IF( AND ((SR_Age__c &gt;= 60), NOT(ISBLANK( Date_Closed__c )) ), 1, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label># Count TGF &gt; 60</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
