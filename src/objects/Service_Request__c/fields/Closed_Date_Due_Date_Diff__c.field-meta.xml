<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Closed_Date_Due_Date_Diff__c</fullName>
    <description>Used to find the difference between closed date and due date is within 30 days</description>
    <externalId>false</externalId>
    <formula>if(text(Status__c)==  &apos;Closed&apos; &amp;&amp; (Customer_Due_Date__c-now() &lt;=30),1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Closed Date Due Date Diff</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
