<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Count_Open_GT_60__c</fullName>
    <description># Count Open &gt; 60 (1 : timebetween(Date Now,Created Date) &gt; 60 Days = True :: 0 otherwise)</description>
    <externalId>false</externalId>
    <formula>IF( AND (((NOW() - CreatedDate )&gt;60), text(Status__c ) &lt;&gt; &apos;Closed&apos;), 1, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label># Count Open &gt; 60</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
