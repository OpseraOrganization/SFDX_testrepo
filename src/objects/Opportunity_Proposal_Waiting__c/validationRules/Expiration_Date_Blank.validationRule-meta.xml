<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expiration_Date_Blank</fullName>
    <active>false</active>
    <errorConditionFormula>And(  
RecordType.DeveloperName = &quot;ISC Waiting&quot;,
not(isBlank(Submitted_to_Pricing_Date__c )),
isBlank(Supplier_Expiration_Date__c )
)</errorConditionFormula>
    <errorMessage>Supplier Expiration Date must be entered</errorMessage>
</ValidationRule>
