<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Proficiency_Level_Value__c</fullName>
    <externalId>false</externalId>
    <formula>IF( 
Ispickval( Employee_Proficiency_Rating__c , &quot;Basic&quot;), 1, 
IF(Ispickval(Employee_Proficiency_Rating__c, &quot;Experienced&quot;), 2, 
IF(Ispickval(Employee_Proficiency_Rating__c, &quot;Advanced&quot;), 3, 
IF(Ispickval(Employee_Proficiency_Rating__c, &quot;Mastery&quot;), 4, 
0) 
) 
)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Proficiency Level Value</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
