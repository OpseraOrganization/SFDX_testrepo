<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case_cannot_change_to_Compliance_Queue</fullName>
    <active>true</active>
    <description>Created for INC000008121557</description>
    <errorConditionFormula>AND(ISPICKVAL(Export_Compliance_Content_ITAR_EAR__c,&apos;No&apos;) ,
ISPICKVAL(Government_Compliance_SM_M_Content__c,&apos;No&apos;),
ISCHANGED(OwnerId), OR(OwnerId = $Label.Compliance_Case_Queue_ID, OwnerId = $Label.Compliance_Queue_CSO_ID, OwnerId = $Label.Compliance_Queue_GTO_ID,OwnerId = $Label.General_Compliance_Queue_Id),
$Profile.Name != $Label.API_data_Load_Profile
)</errorConditionFormula>
    <errorMessage>Cannot change Case ownership to compliance Queues</errorMessage>
</ValidationRule>
