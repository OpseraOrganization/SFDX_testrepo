<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quote_Entered_cannot_be_changed</fullName>
    <active>true</active>
    <errorConditionFormula>( NOT(ISCHANGED(Quote_Number__c)) &amp;&amp; ( Quote_Number__c != null )) &amp;&amp; 
ISCHANGED( Quote_Entered__c )
&amp;&amp; $Profile.Name &lt;&gt; $Label.API_data_Load_Profile</errorConditionFormula>
    <errorDisplayField>Quote_Entered__c</errorDisplayField>
    <errorMessage>You cannot change the Quote Entered value</errorMessage>
</ValidationRule>
