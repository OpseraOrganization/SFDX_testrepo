<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case_Owner_Cannot_change_to_API_Users</fullName>
    <active>false</active>
    <description>Created for INC000007017433</description>
    <errorConditionFormula>ISCHANGED(OwnerId) &amp;&amp;
 CONTAINS($Label.API_User_OwnerId_s,OwnerId) &amp;&amp;
 $Profile.Name != &apos;System Administrator&apos; &amp;&amp; 
 $Profile.Name != &apos;Portal API User - Self-Registration (US)&apos;
 &amp;&amp; $Profile.Name !=&apos;DeniedPartyScreeningAPIUser&apos;
 &amp;&amp; $Profile.Name &lt;&gt; $Label.API_data_Load_Profile</errorConditionFormula>
    <errorMessage>Case Owner cannot be assigned to API Users</errorMessage>
</ValidationRule>
