<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rental_Engine_Serial_Number_reqd_Rented</fullName>
    <active>true</active>
    <description>Rental Engine Serial Number is required when status is rented</description>
    <errorConditionFormula>AND( ISPICKVAL(Status,&apos;Rented&apos;),  ISBLANK(Rental_Engine_Serial_Number__c ),  $RecordType.Name =&apos;Engine Rentals&apos; , $Profile.Name &lt;&gt; $Label.API_data_Load_Profile)</errorConditionFormula>
    <errorDisplayField>Rental_Engine_Serial_Number__c</errorDisplayField>
    <errorMessage>Please enter Rental Engine Serial Number when the status is Rented.</errorMessage>
</ValidationRule>
