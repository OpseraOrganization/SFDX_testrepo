<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ATS_AOG_Cases_All</fullName>
    <booleanFilter>1 AND 2 AND 3 AND (4 OR 5)</booleanFilter>
    <columns>Customer_Request_Date__c</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <columns>Case_Priority__c</columns>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>CASES.STATUS</columns>
    <columns>CASES.CREATED_DATE</columns>
    <columns>NAME</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>CASES.STATUS</field>
        <operation>equals</operation>
        <value>Open,Open-Rejected,Re-Open</value>
    </filters>
    <filters>
        <field>CASES.TYPE</field>
        <operation>equals</operation>
        <value>Technical Issue</value>
    </filters>
    <filters>
        <field>CASES.CREATED_DATE_DATE_ONLY</field>
        <operation>greaterOrEqual</operation>
        <value>1/1/2019 12:00 AM</value>
    </filters>
    <filters>
        <field>Case_Priority__c</field>
        <operation>equals</operation>
        <value>Priority 2 – Priority- Customer Requirement: Closure &lt; 48 hours</value>
    </filters>
    <filters>
        <field>Case_Priority__c</field>
        <operation>equals</operation>
        <value>Priority 1 – AOG/Urgent/Critical: Closure &lt; 24 hours</value>
    </filters>
    <label>ATS AOG Cases - All</label>
    <language>en_US</language>
    <sharedTo>
        <allInternalUsers/>
        <group>ALL_Internal</group>
    </sharedTo>
</ListView>
