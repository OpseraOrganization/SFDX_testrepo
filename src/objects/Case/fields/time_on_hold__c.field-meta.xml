<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>time_on_hold__c</fullName>
    <externalId>false</externalId>
    <formula>IF ( CONTAINS(TEXT(Status),&apos;On Hold&apos;) &amp;&amp; VN_Name__c!=null, time_on_hold_summary__c+round((NOW()- latest_on_hold_start_time__c)*24,2),time_on_hold_summary__c)</formula>
    <label>time on hold</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
