<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OrderEntered__c</fullName>
    <description>Calculates if Order Entered is blank and enters a 0 if true and a 1 if false used for reports only</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Order_Entered__c), 0, 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Order Entered (True/False)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
