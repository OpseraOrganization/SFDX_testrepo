<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TSLI_hr__c</fullName>
    <externalId>false</externalId>
    <formula>if(IsClosed,(ClosedDate-CreatedDate)*24 ,if(isblank(TSLI_DateTime__c),(now()-CreatedDate)*24,(Now()-TSLI_DateTime__c)*24))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Indicates hours how many hrs have lapsed since we communicated with customer</inlineHelpText>
    <label>TSLI (hr)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
