if(and(ispickval(TSLI_threshold__c,&apos;9&apos;),TSLI_hr__c&gt;9),&quot;/img/samples/flag_red.gif&quot;,
if(and(ispickval(TSLI_threshold__c,&apos;6&apos;),TSLI_hr__c&gt;6),&quot;/img/samples/flag_red.gif&quot;,
if(and(ispickval(TSLI_threshold__c,&apos;4&apos;),TSLI_hr__c&gt;4),&quot;/img/samples/flag_red.gif&quot;,
if(and(ispickval(TSLI_threshold__c,&apos;3&apos;),TSLI_hr__c&gt;3),&quot;/img/samples/flag_red.gif&quot;,
if(and(ispickval(TSLI_threshold__c,&apos;1&apos;),TSLI_hr__c&gt;1),&quot;/img/samples/flag_red.gif&quot;,
