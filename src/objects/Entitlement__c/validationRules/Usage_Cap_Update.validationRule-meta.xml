<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Usage_Cap_Update</fullName>
    <active>true</active>
    <errorConditionFormula>ISCHANGED(Usage_Count__c) &amp;&amp; (Usage_Cap__c - Usage_Count__c != Number_Of_Seats__c)</errorConditionFormula>
    <errorMessage>Please enter Correct Usage Count Value</errorMessage>
</ValidationRule>
