<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>APTS_Cancel_CreditMemo__c</fullName>
    <description>Formula field which determines whether to display the Cancel Credit Memo button.  Shown if status is Approved.</description>
    <externalId>false</externalId>
    <formula>IF (
 AND(
   ISPICKVAL(Apttus_Billing__Status__c, &quot;Draft&quot;)
 )
,HYPERLINK(&quot;/apex/APTS_CancelCreditMemo?id=&quot;&amp;Id, IMAGE(&quot;/resource/APTS_Button_CancelCreditMemo&quot;, &quot;Cancel Credit Memo&quot;),&quot;_self&quot;)
,NULL
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Cancel</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
