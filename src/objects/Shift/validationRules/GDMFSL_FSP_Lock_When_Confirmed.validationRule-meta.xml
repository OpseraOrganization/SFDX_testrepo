<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GDMFSL_FSP_Lock_When_Confirmed</fullName>
    <active>false</active>
    <description>DTXFSL-4959</description>
    <errorConditionFormula>AND( 
   ISPICKVAL( Status, &apos;Confirmed&apos;),
   $Profile.Name = &quot;GDMFSL Resource&quot;
)</errorConditionFormula>
    <errorMessage>Once a shift is confirmed, FSP&apos;s cannot change it.</errorMessage>
</ValidationRule>
