<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AccountId</fullName>
    <inlineHelpText>This is the entity that places the PO with Honeywell

View Complete Account list by Selecting first line search</inlineHelpText>
    <lookupFilter>
        <active>false</active>
        <errorMessage>Account Status cannot be Inactive or Temporary.</errorMessage>
        <filterItems>
            <field>Account.Customer_Status__c</field>
            <operation>notEqual</operation>
            <value>Inactive, Temporary</value>
        </filterItems>
        <isOptional>false</isOptional>
    </lookupFilter>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <type>Lookup</type>
</CustomField>
