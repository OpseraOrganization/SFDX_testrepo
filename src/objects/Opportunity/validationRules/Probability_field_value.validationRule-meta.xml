<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Probability_field_value</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ATR_Probability__c &gt;1.0,
$RecordType.Name != &apos;BGA AM&apos;,
$RecordType.Name != &apos;BGA OE&apos; )</errorConditionFormula>
    <errorMessage>The probability field cannot have a value greater than 100%</errorMessage>
</ValidationRule>
