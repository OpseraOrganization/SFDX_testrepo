<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ALL_Operating_Margin</fullName>
    <active>false</active>
    <description>Opportunity operating margin should not be allowed to be greater than 100 (%)</description>
    <errorConditionFormula>OR( 
Operating_Margin__c  &lt; 0, 
Operating_Margin__c &gt; 0.100)</errorConditionFormula>
    <errorDisplayField>Operating_Margin__c</errorDisplayField>
    <errorMessage>“Operating Margin cannot exceed 100%.”</errorMessage>
</ValidationRule>
