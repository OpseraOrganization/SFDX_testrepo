<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HAPP_Enddate_Req_for_StartDate</fullName>
    <active>false</active>
    <description>End date cant null with not null start date</description>
    <errorConditionFormula>AND
(
ISNULL(HAPP_Contract_End_Date__c),
NOT(ISNULL(HAPP_Contract_Start_Date__c )),
$RecordType.Name == &apos;BGA AM&apos;
)</errorConditionFormula>
    <errorDisplayField>HAPP_Contract_End_Date__c</errorDisplayField>
    <errorMessage>Please provide End date, as start date is provided.</errorMessage>
</ValidationRule>
