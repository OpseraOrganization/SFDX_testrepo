<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PSR_Same_Cost_by_Sales_Value_BGs_Percent</fullName>
    <active>true</active>
    <description>Req: 3097, Sprint 10, Pre-Sales.</description>
    <errorConditionFormula>IF(PSR_Automatically_Calc_Estimated_Cost__c = True,
		OR(
		(PSR_Sales_Value_Business_Group_AA__c*VALUE($Label.PSR_Sales_Value_AA_Percentage))/100 = 
		(PSR_Sales_Value_Business_Group_FN__c*VALUE($Label.PSR_Sales_Value_FN_Percentage))/100 ,

		(PSR_Sales_Value_Business_Group_AA__c*VALUE($Label.PSR_Sales_Value_AA_Percentage))/100 = 
		(PSR_Sales_Value_Business_Group_GS__c*VALUE($Label.PSR_Sales_Value_GS_Percentage))/100,

		(PSR_Sales_Value_Business_Group_AA__c*VALUE($Label.PSR_Sales_Value_AA_Percentage))/100 =
		(PSR_Sales_Value_Business_Group_MN__c*VALUE($Label.PSR_Sales_Value_MN_Percentage))/100 ,

		(PSR_Sales_Value_Business_Group_FN__c*VALUE($Label.PSR_Sales_Value_FN_Percentage))/100 = 
		(PSR_Sales_Value_Business_Group_GS__c*VALUE($Label.PSR_Sales_Value_GS_Percentage))/100 ,

		(PSR_Sales_Value_Business_Group_FN__c*VALUE($Label.PSR_Sales_Value_FN_Percentage))/100 = 
		(PSR_Sales_Value_Business_Group_MN__c*VALUE($Label.PSR_Sales_Value_MN_Percentage))/100 , 

		(PSR_Sales_Value_Business_Group_GS__c*VALUE($Label.PSR_Sales_Value_GS_Percentage))/100 = 
		(PSR_Sales_Value_Business_Group_MN__c*VALUE($Label.PSR_Sales_Value_MN_Percentage))/100 

		),null)</errorConditionFormula>
    <errorMessage>The Sales Value BG&apos;s you entered leads to a Double cost</errorMessage>
</ValidationRule>
