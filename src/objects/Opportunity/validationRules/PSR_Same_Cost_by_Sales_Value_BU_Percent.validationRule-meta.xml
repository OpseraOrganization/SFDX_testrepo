<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PSR_Same_Cost_by_Sales_Value_BU_Percent</fullName>
    <active>true</active>
    <errorConditionFormula>IF(
PSR_Automatically_Calc_Estimated_Cost__c= True,
OR(
(PSR_Sales_Value_Business_Unit_ION_IPR__c*VALUE($Label.PSR_Sales_Value_ION_IPR_Percentage))/100 = 
(PSR_Sales_Value_Business_Unit_ION_Optics__c*VALUE($Label.PSR_Sales_Value_ION_Optics_Percentage))/100,

(PSR_Sales_Value_Business_Unit_ION_IPR__c*VALUE($Label.PSR_Sales_Value_ION_IPR_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_DNI__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_DNI_Percentage))/100,

(PSR_Sales_Value_Business_Unit_ION_IPR__c*VALUE($Label.PSR_Sales_Value_ION_IPR_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_MPC__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_MPC_Percentage))/100,

(PSR_Sales_Value_Business_Unit_ION_IPR__c*VALUE($Label.PSR_Sales_Value_ION_IPR_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_Nuage__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_Nuage_Percentage))/100,

(PSR_Sales_Value_Business_Unit_ION_IPR__c*VALUE($Label.PSR_Sales_Value_ION_IPR_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_Services__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_Services_Percentage))/100,

(PSR_Sales_Value_Business_Unit_ION_IPR__c*VALUE($Label.PSR_Sales_Value_ION_IPR_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_Optics_Services__c*VALUE($Label.PSR_Sales_Value_BU_ION_Optics_Services_Percentage))/100,

(PSR_Sales_Value_Business_Unit_ION_Optics__c*VALUE($Label.PSR_Sales_Value_ION_Optics_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_DNI__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_DNI_Percentage))/100,

(PSR_Sales_Value_Business_Unit_ION_Optics__c*VALUE($Label.PSR_Sales_Value_ION_Optics_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_MPC__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_MPC_Percentage))/100,

(PSR_Sales_Value_Business_Unit_ION_Optics__c*VALUE($Label.PSR_Sales_Value_ION_Optics_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_Nuage__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_Nuage_Percentage))/100,

(PSR_Sales_Value_Business_Unit_ION_Optics__c*VALUE($Label.PSR_Sales_Value_ION_Optics_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_Services__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_Services_Percentage))/100,

(PSR_Sales_Value_Business_Unit_ION_Optics__c*VALUE($Label.PSR_Sales_Value_ION_Optics_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_Optics_Services__c*VALUE($Label.PSR_Sales_Value_BU_ION_Optics_Services_Percentage))/100,

(PSR_Sales_Value_BU_ION_IPR_DNI__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_DNI_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_MPC__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_MPC_Percentage))/100,

(PSR_Sales_Value_BU_ION_IPR_DNI__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_DNI_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_Nuage__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_Nuage_Percentage))/100,

(PSR_Sales_Value_BU_ION_IPR_DNI__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_DNI_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_Services__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_Services_Percentage))/100,

(PSR_Sales_Value_BU_ION_IPR_DNI__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_DNI_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_Optics_Services__c*VALUE($Label.PSR_Sales_Value_BU_ION_Optics_Services_Percentage))/100,

(PSR_Sales_Value_BU_ION_IPR_MPC__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_MPC_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_Nuage__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_Nuage_Percentage))/100,

(PSR_Sales_Value_BU_ION_IPR_MPC__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_MPC_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_IPR_Services__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_Services_Percentage))/100,

(PSR_Sales_Value_BU_ION_IPR_MPC__c*VALUE($Label.PSR_Sales_Value_BU_ION_IPR_MPC_Percentage))/100 = 
(PSR_Sales_Value_BU_ION_Optics_Services__c*VALUE($Label.PSR_Sales_Value_BU_ION_Optics_Services_Percentage))/100
),null)</errorConditionFormula>
    <errorMessage>The Sales Value in ION / NEN section you entered leads to a Double cost</errorMessage>
</ValidationRule>
