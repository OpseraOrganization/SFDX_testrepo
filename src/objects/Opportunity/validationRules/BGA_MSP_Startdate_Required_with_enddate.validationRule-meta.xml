<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BGA_MSP_Startdate_Required_with_enddate</fullName>
    <active>false</active>
    <description>Start date cant null with not null end date</description>
    <errorConditionFormula>AND
(
ISNULL(MSP_Contract_End_Date__c),
NOT(ISNULL(MSP_Contract_Start_Date__c)),
$RecordType.Name == &apos;BGA AM&apos;
)</errorConditionFormula>
    <errorDisplayField>HAPP_Contract_Start_Date__c</errorDisplayField>
    <errorMessage>Please provide Start date, as end date is provided.</errorMessage>
</ValidationRule>
