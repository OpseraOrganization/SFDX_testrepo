<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GM_not_greater_than_100</fullName>
    <active>false</active>
    <errorConditionFormula>AND(GM__c &gt;1.0,$RecordType.Name != &apos;BGA AM&apos;,$RecordType.Name != &apos;BGA OE&apos; )</errorConditionFormula>
    <errorDisplayField>GM__c</errorDisplayField>
    <errorMessage>Please enter a value less than 100%</errorMessage>
</ValidationRule>
