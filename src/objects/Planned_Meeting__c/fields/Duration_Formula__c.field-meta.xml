<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Duration_Formula__c</fullName>
    <description>Duration needs to be a formula field, the duration number field should be deleted from the page layout.</description>
    <externalId>false</externalId>
    <formula>TEXT(ROUND((End__c - Start__c) * 24, 2))+&apos; Hours&apos;</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Duration</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
