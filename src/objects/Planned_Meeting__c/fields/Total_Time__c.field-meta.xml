<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Time__c</fullName>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(Date_Time_Ended__c) || ISBLANK(Date_Time_Started__c)), (floor((Date_Time_Ended__c - Date_Time_Started__c)*24))
+
(((((Date_Time_Ended__c - Date_Time_Started__c)*24)-floor((Date_Time_Ended__c - Date_Time_Started__c)*24))*60)/100),Null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
