<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Proposal_Due_Date_Required</fullName>
    <active>true</active>
    <errorConditionFormula>OR(  AND( NOT(isBLANK( RFP_Receipt_Date__c)),   

(isBLANK(Proposal_Due_Date__c))) 
)</errorConditionFormula>
    <errorDisplayField>Proposal_Due_Date__c</errorDisplayField>
    <errorMessage>Proposal Due Date is required when proposal has been received.</errorMessage>
</ValidationRule>
