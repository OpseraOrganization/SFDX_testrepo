<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DatesFuture_Kickoff</fullName>
    <active>true</active>
    <errorConditionFormula>OR( 
Kickoff_Meeting_Date__c &gt; Today() 
)</errorConditionFormula>
    <errorDisplayField>Kickoff_Meeting_Date__c</errorDisplayField>
    <errorMessage>The Kickoff Meeting Date cannot be in the future.  Please enter the actual date this activity happened.</errorMessage>
</ValidationRule>
