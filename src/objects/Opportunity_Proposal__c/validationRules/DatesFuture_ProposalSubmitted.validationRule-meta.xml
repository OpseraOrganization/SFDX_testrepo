<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DatesFuture_ProposalSubmitted</fullName>
    <active>true</active>
    <errorConditionFormula>OR( 
Proposal_Submitted_Date__c &gt; today()
)</errorConditionFormula>
    <errorDisplayField>Proposal_Submitted_Date__c</errorDisplayField>
    <errorMessage>The Proposal Submitted Date cannot be in the future.  Please enter the actual date this activity happened.</errorMessage>
</ValidationRule>
