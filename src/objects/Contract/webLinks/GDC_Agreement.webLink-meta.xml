<?xml version="1.0" encoding="UTF-8"?>
<WebLink xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GDC_Agreement</fullName>
    <availability>online</availability>
    <description>Creates GDC Agreement using Conga and EchoSign</description>
    <displayType>button</displayType>
    <encodingKey>UTF-8</encodingKey>
    <hasMenubar>false</hasMenubar>
    <hasScrollbars>false</hasScrollbars>
    <hasToolbar>false</hasToolbar>
    <height>500</height>
    <isResizable>true</isResizable>
    <linkType>url</linkType>
    <masterLabel>GDC Agreement</masterLabel>
    <openType>newWindow</openType>
    <position>topLeft</position>
    <protected>false</protected>
    <showsLocation>false</showsLocation>
    <showsStatus>false</showsStatus>
    <url>https://www.appextremes.com/apps/Conga/PointMerge.aspx
?sessionId={!API.Session_ID}
&amp;serverUrl={!API.Partner_Server_URL_80}
&amp;id={!Contract.Id}
&amp;TemplateGroup={!Contract.TemplateGroup__c}
&amp;ESContactId={!Contract.Renewal_Order_Receipt_NameId__c}
&amp;ESCopyTo=john.lagos@honeywell.com
&amp;ESSignatureType=2
&amp;ESRequireSelfSign=1
&amp;ESSignatureOrder=1
&amp;ESRemindRecipient=2
&amp;ESAgreementName={!Contract.Sub_Classification__c}+Agmt:+{!Contract.ContractNumber}+{!Contract.CreatedDate}
&amp;ESMessage=Honeywell+{!Contract.Sub_Classification__c}+Agreement.
&amp;DS7=4
&amp;DS0=1
&amp;LG1=GDC+Generated
&amp;DefaultPDF=1
&amp;ESVisible=1
&amp;LG0=0&amp;LG3=2&amp;LGAttachOption=0
&amp;SelectTemplates=1
&amp;OFN=GDC+Contract+{!Contract.ContractNumber}+{!Contract.CreatedDate}</url>
    <width>700</width>
</WebLink>
