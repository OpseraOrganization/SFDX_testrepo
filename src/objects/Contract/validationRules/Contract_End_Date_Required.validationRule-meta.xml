<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contract_End_Date_Required</fullName>
    <active>false</active>
    <description>If End Date Required = Yes, EndDate field becomes required.</description>
    <errorConditionFormula>AND( ISPICKVAL(End_date_Required__c,&apos;Yes&apos; ), ISBLANK( EndDate ))</errorConditionFormula>
    <errorMessage>Please enter a End Date</errorMessage>
</ValidationRule>
