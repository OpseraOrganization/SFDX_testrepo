<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Eligible_Offering_Future_Date</fullName>
    <active>true</active>
    <description>Account_Eligible_Offering_Future_Date</description>
    <errorConditionFormula>and(
 text( Status__c ) = &quot;Future&quot;,
 ISNULL( Future_Review_Date__c ) 
)</errorConditionFormula>
    <errorDisplayField>Status__c</errorDisplayField>
    <errorMessage>Future Review Date must be entered when future is choosen.</errorMessage>
</ValidationRule>
