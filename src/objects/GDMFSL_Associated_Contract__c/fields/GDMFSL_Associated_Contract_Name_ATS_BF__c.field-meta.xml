    <formula>Service_Contract__r.Name &amp; &quot; - Remaining ATS BF: &quot; &amp;  TEXT(ROUND(Service_Contract__r.GDMFSL_Remaining_BF_Hours__c, 2))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Associated Contract Name ATS BF</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
