<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Public_External</fullName>
    <active>false</active>
    <errorConditionFormula>AND(Publish_External__c , ISBLANK( Intellectual_Property_Review_Date__c ) )</errorConditionFormula>
    <errorDisplayField>Intellectual_Property_Review_Date__c</errorDisplayField>
    <errorMessage>Intellectual Property Review Date Required only if the Publish External checkbox is selected</errorMessage>
</ValidationRule>
