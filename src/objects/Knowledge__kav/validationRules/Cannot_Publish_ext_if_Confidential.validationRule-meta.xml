<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_Publish_ext_if_Confidential</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
RecordType.DeveloperName=&quot;Knowledge_Article&quot;,
OR(IsVisibleInPkb,IsVisibleInCsp,IsVisibleInPrm), R_C__c &lt;&gt;&quot;&quot;)</errorConditionFormula>
    <errorMessage>Article cannot be published externally (channels selection) and marked confidential at the same time. Please update accordingly</errorMessage>
</ValidationRule>
