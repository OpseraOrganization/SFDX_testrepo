<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Title_and_Summary_Cannot_be_Same</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
RecordType.DeveloperName=&quot;Knowledge_Article&quot;,
LOWER(Summary) == LOWER(Title))</errorConditionFormula>
    <errorMessage>Title and Summary fields cannot be same. Please fill summary with few keywords.</errorMessage>
</ValidationRule>
