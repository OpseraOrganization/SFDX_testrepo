<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ParentId</fullName>
    <inlineHelpText>A related organization which is higher than this account in the organizational hierarchy.</inlineHelpText>
    <lookupFilter>
        <active>false</active>
        <errorMessage>Parent Account Status should not be inactive</errorMessage>
        <filterItems>
            <field>Account.Customer_Status__c</field>
            <operation>notEqual</operation>
            <value>Inactive</value>
        </filterItems>
        <isOptional>false</isOptional>
    </lookupFilter>
    <trackFeedHistory>true</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <type>Hierarchy</type>
</CustomField>
