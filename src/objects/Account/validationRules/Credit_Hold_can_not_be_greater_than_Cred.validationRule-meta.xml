<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Credit_Hold_can_not_be_greater_than_Cred</fullName>
    <active>false</active>
    <description>Credit Hold can not be greater than Credit Limit.</description>
    <errorConditionFormula>AND(
Credit_Limit__c&gt;0,
Credit_Hold__c&gt;0,
Credit_Limit__c &lt; Credit_Hold__c
)</errorConditionFormula>
    <errorDisplayField>Credit_Hold__c</errorDisplayField>
    <errorMessage>Credit Hold can not be greater than Credit Limit.</errorMessage>
</ValidationRule>
