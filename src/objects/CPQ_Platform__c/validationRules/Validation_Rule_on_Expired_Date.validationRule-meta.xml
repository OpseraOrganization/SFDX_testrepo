<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_Rule_on_Expired_Date</fullName>
    <active>true</active>
    <errorConditionFormula>DATEVALUE(License_Expiry_Date__c) &lt; TODAY()</errorConditionFormula>
    <errorMessage>License Expiry date should not be less than today date</errorMessage>
</ValidationRule>
