<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mandate_Contact_Information</fullName>
    <active>false</active>
    <description>Makes user to enter the Contact information and the Account gets auto populated based on the Contact selected.</description>
    <errorConditionFormula>Case_Number__c == null &amp;&amp;  Contact_Name__c ==null</errorConditionFormula>
    <errorDisplayField>Contact_Name__c</errorDisplayField>
    <errorMessage>Please enter Contact information.</errorMessage>
</ValidationRule>
