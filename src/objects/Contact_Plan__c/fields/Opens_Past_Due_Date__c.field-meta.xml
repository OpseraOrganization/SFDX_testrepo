<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Opens_Past_Due_Date__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
AND( Expected_Date__c &lt;  TODAY() ,
            NOT(ISPICKVAL( Status__c , &apos;Done&apos;) ||  ISPICKVAL( Status__c , &apos;Cancelled&apos;))
),1,0)</formula>
    <label>Open Past Due Date</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
