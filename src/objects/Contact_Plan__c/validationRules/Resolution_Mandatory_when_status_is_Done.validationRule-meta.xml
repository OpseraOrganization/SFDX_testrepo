<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Resolution_Mandatory_when_status_is_Done</fullName>
    <active>true</active>
    <description>Resolution_Mandatory_when_status_is_Done</description>
    <errorConditionFormula>AND(ISPICKVAL( Status__c , &apos;Done&apos;) , ISBLANK( Resolution__c ) )</errorConditionFormula>
    <errorDisplayField>Resolution__c</errorDisplayField>
    <errorMessage>Please Enter Resolution</errorMessage>
</ValidationRule>
