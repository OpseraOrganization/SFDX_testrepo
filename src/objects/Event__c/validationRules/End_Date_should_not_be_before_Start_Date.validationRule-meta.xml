<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_should_not_be_before_Start_Date</fullName>
    <active>true</active>
    <description>End_Date_should_not_be_before_Start_Date</description>
    <errorConditionFormula>End_Date__c &lt; Start_Date__c</errorConditionFormula>
    <errorDisplayField>End_Date__c</errorDisplayField>
    <errorMessage>Please enter an End Date greater than Start Date</errorMessage>
</ValidationRule>
