<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MyMaintainer_Roles_Cant_Blank_on_Approve</fullName>
    <active>false</active>
    <description>Created for MyMaintainer Portal Registration Project</description>
    <errorConditionFormula>ISBLANK(TEXT(MyMaintainer_Roles__c)) &amp;&amp;  TEXT( Request_Status__c) = &apos;Approved&apos; &amp;&amp;  Portal_Tool_Master__c = $Label.MyMaintainer_Tool_ID</errorConditionFormula>
    <errorDisplayField>MyMaintainer_Roles__c</errorDisplayField>
    <errorMessage>MyMaintainer Roles field cannot be blank on Request Status set to Approved</errorMessage>
</ValidationRule>
