    <html><head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8;width=device-width"/>
        </head>
        <body >
            <div id="Section 1" class="mktEditable,smartphone,laptop" >
                <table style="font-family: Arial;font-size: 14px;" border="0" width="600" cellspacing="0" cellpadding="0" align="center">
                    
                    <table border="0" width="600" cellspacing="0" cellpadding="0">
                        <tbody>
                            <tr colspan="4" width="600">
                                <td colspan="2" >
                                    <img src="https://hon-aero--c.documentforce.com/servlet/servlet.ImageServer?id=01513000002momf&oid=00D30000000dWxY" width="200" class="header"/>
                                </td>
                                <td></td>
                                <td  colspan="2" align="right"  style="font-family: Arial;font-size: 12px;text-align:right;" > 
                                    <p align="right"><i style="text-align:right; font-size: 14px;"> 
                                        <apex:outputText value="Date: {0,date,MM/dd/yyyy}">
                                            <apex:param value="{!relatedTo.Today_Date__c}" />
                                        </apex:outputText><br/> Case Number: {!relatedTo.CaseNumber} <br/>
                                        Honeywell Sales Order: {!relatedTo.Honeywell_Sales_Order__c} <br /> 
                                        PO#: {!relatedTo.Customer_PO_RO_WONumber__c} </i></p>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <table border="0" width="600" cellspacing="0" cellpadding="0">
                        <tbody>
                            <tr colspan="4" width="600">
                                <td  width="600" >
