public class CustomOppProductDetailController{
    public string businessProcessName{get;set;}
    public List<Schema.FieldSetMember> fields{get;set;}
    public List<Schema.FieldSetMember> fields_LHS{get;set;}
    public List<Schema.FieldSetMember> fields_RHS{get;set;}
    public List<Schema.FieldSetMember> pricingFields{get;set;}
    public List<Schema.FieldSetMember> SystemFields{get;set;}
    public List<Schema.FieldSetMember> infoFields{get;set;}
    public List<Schema.FieldSetMember> DSTSFields{get;set;}
    public List<Schema.FieldSetMember> DSProductFields{get;set;}
    public OpportunityLineItem opliRec{get;set;}
    public boolean showClone{get;set;}
    public boolean showEdit{get;set;}
    public boolean showDelete{get;set;}
    public String fldSetName{get;set;}
    public String hasSchedule{get;set;}
    public String mess{get;set;}
    public CustomOppProductDetailController(ApexPages.StandardController stdController) {
        showClone = true;
        showEdit = true;
        showDelete = true;
        
        OpportunityLineItem oppProductLine = [select Opportunity.recordType.BusinessProcessID,Has_Schedule__c from OpportunityLineItem 
                                                        where id = :stdController.getRecord().Id];   
        hasSchedule = oppProductLine.Has_Schedule__c;                                           
        opliRec = new OpportunityLineItem();    
        ID processId = oppProductLine.Opportunity.recordType.BusinessProcessID;
        businessProcessName = 'None';
        fldSetName = 'None';
        if(processId != null){
           BusinessProcess bp = [select name from BusinessProcess where id=:processId];
           businessProcessName = bp.Name; 
           fldSetName = businessProcessName.substring(0,3);
           fldSetName = fldSetName.replaceAll('&','_');
        }
        
        if(!OpportunityLineItem.sObjectType.getDescribe().isDeletable()){
            showDelete = false;
        }
        if(!OpportunityLineItem.sObjectType.getDescribe().isUpdateable()){
            showEdit = false;
        }
        if(!OpportunityLineItem.sObjectType.getDescribe().isCreateable()){
            showClone = false;
        }
        assignFieldSetMembers(fldSetName,stdController.getRecord().Id); 
        system.debug('businessProcessName value:'+businessProcessName);
    }
    
    private void assignFieldSetMembers(String fldSetName,id recId){
        try{
            string selectFlds = '';
            list<Schema.FieldSetMember> totalFields = new list<Schema.FieldSetMember>();           
            if(Schema.SObjectType.OpportunityLineItem.fieldSets.getMap().get(fldSetName) != null){
                fields = Schema.SObjectType.OpportunityLineItem.fieldSets.getMap().get(fldSetName).getFields();
                totalFields.addAll(fields);
                fields_LHS = Schema.SObjectType.OpportunityLineItem.fieldSets.getMap().get(fldSetName+'_LHS').getFields();
                totalFields.addAll(fields_LHS);
                fields_RHS = Schema.SObjectType.OpportunityLineItem.fieldSets.getMap().get(fldSetName+'_RHS').getFields();
                totalFields.addAll(fields_RHS);
            }else{
                fields = Schema.SObjectType.OpportunityLineItem.fieldSets.getMap().get('None').getFields();
                totalFields.addAll(fields);
            }
            
            SystemFields = Schema.SObjectType.OpportunityLineItem.fieldSets.getMap().get('SystemFields').getFields();
            totalFields.addAll(SystemFields);
            if(fldSetName == 'BGA'){
                pricingFields = Schema.SObjectType.OpportunityLineItem.fieldSets.getMap().get(fldSetName+'_Pricing').getFields();
                infoFields = Schema.SObjectType.OpportunityLineItem.fieldSets.getMap().get(fldSetName+'_Info').getFields();
                totalFields.addAll(pricingFields);
                totalFields.addAll(infoFields);
            }
            if(fldSetName == 'D_S' || Test.isRunningTest()){
                 system.debug('DSTSFields value:'+DSTSFields);
                DSTSFields= Schema.SObjectType.OpportunityLineItem.fieldSets.getMap().get('D_S_TS').getFields();
                system.debug('DSTSFields value:'+DSTSFields);
                DSProductFields = Schema.SObjectType.OpportunityLineItem.fieldSets.getMap().get('D_S_Product').getFields();
                totalFields.addAll(DSTSFields);
                totalFields.addAll(DSProductFields);
            }
           
          for(Schema.FieldSetMember fldMember:totalFields){
           if(selectFlds.length()>0){
               selectFlds = selectFlds+','+fldMember.getFieldPath();
           }else{
               selectFlds = selectFlds+fldMember.getFieldPath();
           }           
          }
          string queryS = 'select '+selectFlds+' from OpportunityLineItem where id=:recId limit 1';
          opliRec = Database.query(queryS);
          system.debug('opliRec value:'+opliRec);
        }catch(Exception e){
            system.debug('Exception at assignFieldSetMembers'+e.getMessage());
        }
    }
    
    public PageReference deleteRec(){
        try{
            PageReference ret = new PageReference('/'+opliRec.OpportunityId);
            id recId = ApexPages.currentPage().getParameters().get('id');
            OpportunityLineItem oppProdRec = new OpportunityLineItem();
            oppProdRec.id = recId;
            delete oppProdRec;
            ret.setRedirect(true);
            return ret;       
        }catch(Exception e){
            system.debug('Exception during record delete:'+e.getMessage());
            return null;
        }
    }
    
    public PageReference deleteScheduleRec(){
       
        try{       
            id recId = ApexPages.currentPage().getParameters().get('id');
            PageReference ret = new PageReference('/'+recId);
            Delete [SELECT id from OpportunityLineItemSchedule where OpportunityLineItemId = :recId ];
            ret.setRedirect(true);
            return ret;   
                
        }catch(Exception e){
            system.debug('Exception during record delete:'+e.getMessage());
            return null;
        }
       
    }
    
    public PageReference save(){
        try{
            update opliRec;
            
        }catch(Exception e){
            ApexPages.Message myMsg = new ApexPages.Message(ApexPages.Severity.ERROR,'Error: '+e.getMessage());
            ApexPages.addMessage(myMsg);
            system.debug('Exception during save:'+e.getMessage());
            return null;
        }
        string returl=ApexPages.currentPage().getParameters().get('retURL');
        if(returl == null && returl == '')
        {
           returl='/apex/CustomOppProductDetailPage?id='+opliRec.id+'&sfdc.override=1';
        }
         LIST<OpportunityLineItemSchedule> oppLineSch = [SELECT Id, OpportunityLineItemId FROM OpportunityLineItemSchedule WHERE OpportunityLineItemId =: opliRec.id];
         
         PageReference ret;
         if(oppLineSch.size() == 0){
             mess = 'Please Create a Revenue Schedule';
         }
         System.debug(mess +'oppLineSch ==='+oppLineSch.size());
         ret = new PageReference('/apex/CustomOppProductDetailPage?id='+opliRec.id+'&sfdc.override=1');
        ret.setRedirect(true);
        system.debug('Save is success');
        return ret;  
    }
     
     public PageReference cancel(){
        try{
            PageReference ret = new PageReference('/'+opliRec.OpportunityId);
            ret.setRedirect(true);
            return ret;  
        }
        catch(Exception e){
            ApexPages.Message myMsg = new ApexPages.Message(ApexPages.Severity.ERROR,'Error: '+e.getMessage());
            ApexPages.addMessage(myMsg);
            system.debug('Exception during cancel:'+e.getMessage());
            return null;
        }
       
    }
