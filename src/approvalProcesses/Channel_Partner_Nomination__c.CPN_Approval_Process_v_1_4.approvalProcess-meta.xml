    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Contract_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>Licensing_Approval__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>AMB_Approver_M_SC__c</name>
