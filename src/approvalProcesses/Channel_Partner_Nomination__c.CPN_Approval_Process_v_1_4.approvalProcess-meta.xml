    <description>This is created for Channel Partner Nomination</description>
    <emailTemplate>Channel_Partner_Nomination/Notify_Approver_about_CPN_approval</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <booleanFilter>1 AND 2 AND 3 AND 4 AND 5 AND 6 AND 7</booleanFilter>
        <criteriaItems>
            <field>Channel_Partner_Nomination__c.Type_of_Agreement__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Channel_Partner_Nomination__c.SAP_Account__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Channel_Partner_Nomination__c.STC_Applicable_to_the_Partner__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Channel_Partner_Nomination__c.Product_s_Service_s_in_Scope__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Channel_Partner_Nomination__c.Is_Due_Diligence_Required_under_Policy_2__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Channel_Partner_Nomination__c.Reduction_of_Work_at_Site__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Channel_Partner_Nomination__c.Check_Attachment__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
