            </criteriaItems>
            <criteriaItems>
                <field>Channel_Partner_Nomination__c.Vertical_Strategy_Approvre__c</field>
                <operation>notEqual</operation>
            </criteriaItems>
            <criteriaItems>
                <field>Channel_Partner_Nomination__c.Licensing_Approval__c</field>
                <operation>notEqual</operation>
            </criteriaItems>
            <criteriaItems>
                <field>Channel_Partner_Nomination__c.AMB_Approver__c</field>
                <operation>notEqual</operation>
                <value>Null</value>
            </criteriaItems>
            <criteriaItems>
                <field>Channel_Partner_Nomination__c.AMB_Approver_E_PS__c</field>
                <operation>notEqual</operation>
                <value>Null</value>
            </criteriaItems>
            <criteriaItems>
                <field>Channel_Partner_Nomination__c.AMB_Approver_M_SC__c</field>
                <operation>notEqual</operation>
                <value>Null</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>RejectRecord</ifCriteriaNotMet>
        <label>Approvals from various users</label>
        <name>Approvals_from_various_users</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>jan.strzebniok@honeywell.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Channel_Partner_Nomination__c.Reduction_of_Work_at_Site__c</field>
                <operation>notEqual</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>ApproveRecord</ifCriteriaNotMet>
        <label>Legal Approval</label>
        <name>Legal_Approval</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Secondary_Approval_for_CM__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Channel_Partner_Nomination__c.Secondary_Approval_for_CM__c</field>
                <operation>notEqual</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>ApproveRecord</ifCriteriaNotMet>
