            </approver>
            <approver>
                <name>AMB_Approver_E_PS__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>AMB_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>Vertical_Strategy_Approvre__c</name>
                <type>relatedUserField</type>
            </approver>
            <approver>
                <name>shevon.quijano@honeywell.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <booleanFilter>1 AND 2 AND 3 AND (4 OR 5 OR 6)</booleanFilter>
            <criteriaItems>
                <field>Channel_Partner_Nomination__c.Contract_Approver__c</field>
